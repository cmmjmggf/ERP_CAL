	<parameter name="FECHA_FINAL" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT     IFNULL((SELECT FF.precto FROM facturacion AS FF WHERE FF.contped = D.control AND FF.cliente = D.cliente AND ff.estilo = D.estilo ORDER BY FF.ID DESC LIMIT 1),0)   AS PRECIO ,
 D.fecha, D.clasif AS CLASIFICACION,
(CASE
WHEN D.clasif = 1 THEN "***** PARA VENTA *****"
WHEN D.clasif = 2 THEN "***** SALDOS *****"
WHEN D.clasif = 3 THEN "***** REPARACION *****"
 END) AS CLASIFICACION_T,
 D.seriped AS SERIE, D.cliente AS CLIENTE_CLAVE,
(SELECT CC.RazonS FROM clientes AS CC WHERE CC.Clave = D.cliente)  AS CLIENTE_DOS,
D.control AS CONTROL,
(CASE WHEN D.estilo IS NULL OR D.estilo =''  OR D.estilo ='0'  OR D.estilo =0 THEN P.Estilo ELSE D.estilo END) AS ESTILO_CLAVE,
(CASE WHEN D.comb IS NULL  OR D.comb =''  OR D.comb ='0'  OR D.comb =0  THEN P.Color ELSE D.comb END)  AS COLOR_CLAVE,

(SELECT CO.Descripcion FROM colores AS CO WHERE (CASE WHEN D.comb IS NULL  OR D.comb =''  OR D.comb ='0'  OR D.comb =0  THEN P.Color ELSE D.comb END) = CO.Clave  AND (CASE WHEN D.estilo IS NULL OR D.estilo =''  OR D.estilo ='0'  OR D.estilo =0 THEN P.Estilo ELSE D.estilo END) = CO.Estilo LIMIT 1) AS COLOR,
D.paredev AS PARES_DEVUELTOS,

D.ctenvo AS DEPARTAMENTO_CLAVE,
(SELECT DEP.Descripcion FROM departamentos AS DEP WHERE D.ctenvo = DEP.Clave) as DEPARTAMENTO_DOS,

D.defecto AS DEFECTO_CLAVE,
(SELECT DEF.Descripcion FROM defectos AS DEF WHERE D.defecto = DEF.Clave  LIMIT 1) AS DEFECTO_DOS,

D.detalle AS DETALLE_CLAVE,
(SELECT DD.Descripcion FROM defectosdetalle AS DD WHERE D.detalle = DD.Clave LIMIT 1) AS DETALLE_DOS,

D.staapl AS STATUS_APLICADA, (CASE WHEN D.cargoa = 0 THEN "NO" WHEN D.cargoa = 1 THEN "PRV" WHEN D.cargoa = 2 THEN "MAQ" END) AS CARGO_A, (D.paredev * D.preciodev) AS VTA_PARES, D.preciodev AS DEVO, (D.preciodev * D.paredev) AS DEV_PARES_NOVNTA,
((D.paredev * P.Precio) - (D.preciodev * D.paredev)) AS DIFER, D.maq AS MAQUILA, D.maq AS MAQUILA_CLAVE, (SELECT M.Nombre FROM maquilas AS M WHERE M.Clave = D.maq LIMIT 1) AS MAQUILA_DES
FROM devolucionnp as D INNER JOIN clientes AS C ON D.cliente = C.Clave
INNER JOIN pedidox AS P ON D.control = P.Control
WHERE  D.fecha BETWEEN str_to_date($P{FECHA_INICIAL},"%d/%m/%Y") AND str_to_date($P{FECHA_FINAL},"%d/%m/%Y")
AND D.cargoa = 2 ORDER BY ABS(D.maq) ASC, CLIENTE_DOS ASC;]]>
	</queryString>
