	<queryString>
		<![CDATA[SELECT   P.Precio  AS PRECIO ,
 D.fecha, DATE_FORMAT(D.fecha,"%d/%m/%Y") AS FECHA_F, D.clasif AS CLASIFICACION,
(CASE
WHEN D.clasif = 1 THEN "***** PARA VENTA *****"
WHEN D.clasif = 2 THEN "***** SALDOS *****"
WHEN D.clasif = 3 THEN "***** REPARACION *****"
 END) AS CLASIFICACION_T,
 D.seriped AS SERIE, D.cliente AS CLIENTE_CLAVE,
(CASE WHEN D.tp = 1 THEN C.RazonS WHEN D.tp = 2 THEN C.NombreP END) AS CLIENTE_DOS,
D.control AS CONTROL,
D.estilo AS ESTILO_CLAVE,
D.comb AS COLOR_CLAVE, (SELECT CO.Descripcion FROM colores AS CO WHERE D.comb = CO.Clave  AND D.estilo = CO.Estilo LIMIT 1) AS COLOR,
D.paredev AS PARES_DEVUELTOS,

D.ctenvo AS DEPARTAMENTO_CLAVE,
(SELECT (CASE WHEN DEP.Descripcion IS NULL THEN "SIN DEPTO" ELSE DEP.Descripcion END) FROM departamentos AS DEP WHERE D.ctenvo = DEP.Clave) as DEPARTAMENTO_DOS,

D.defecto AS DEFECTO_CLAVE,
(SELECT DEF.Descripcion FROM defectos AS DEF WHERE D.defecto = DEF.Clave  LIMIT 1) AS DEFECTO_DOS,

D.detalle AS DETALLE_CLAVE,
(SELECT DD.Descripcion FROM defectosdetalle AS DD WHERE D.detalle = DD.Clave LIMIT 1) AS DETALLE_DOS,

(CASE WHEN D.staapl = 1 THEN "S" ELSE "N" END) AS STATUS_APLICADA,

(CASE WHEN D.cargoa = 0 THEN "NO" WHEN D.cargoa = 1 THEN "PRV" WHEN D.cargoa = 2 THEN "MAQ" END) AS CARGO_A, (D.paredev * P.Precio) AS VTA_PARES, 90 AS DEVO, (90 * D.paredev) AS DEV_PARES_NOVNTA,
((D.paredev * P.Precio) - (90 * D.paredev)) AS DIFER, D.maq AS MAQUILA, D.maq AS MAQUILA_CLAVE, (SELECT M.Nombre FROM maquilas AS M WHERE M.Clave = D.maq LIMIT 1) AS MAQUILA_DES,
(CASE
WHEN D.stafac = 0 THEN "Nf"
WHEN D.stafac = 1 THEN "Fp"
WHEN D.stafac = 2 THEN "Fc"
END)
AS ESTATUS_FACTURA

FROM devolucionnp as D INNER JOIN clientes AS C ON D.cliente = C.Clave
INNER JOIN pedidox AS P ON D.control = P.Control
WHERE  D.fecha BETWEEN str_to_date($P{FECHA_INICIAL},"%d/%m/%Y") AND str_to_date($P{FECHA_FINAL},"%d/%m/%Y")
ORDER BY ABS(D.ctenvo) ASC,ABS(D.defecto) ASC,ABS(D.detalle) ASC, D.fecha DESC;]]>
	</queryString>
