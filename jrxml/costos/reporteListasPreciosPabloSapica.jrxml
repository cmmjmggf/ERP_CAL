	<parameter name="lista" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
cv.lista,
lp.Descripcion as nomlista,
case when cv.corr = 0 then 'Sin corrida'
when cv.corr = 1 then '17/21½'
when cv.corr = 2 then '22/27'
when cv.corr = 3 then '25/31'
when cv.corr = 4 then '17/21'
when cv.corr = 5 then '5/11'
when cv.corr = 6 then '6/13' end as corrida,
case when cv.corr = 0 then 'Sin corrida'
when cv.corr = 1 then 'Niño'
when cv.corr = 2 then 'Dama'
when cv.corr = 3 then 'Hombre'
when cv.corr = 4 then 'Niño E'
when cv.corr = 5 then 'Dama E'
when cv.corr = 6 then 'Hombre E' end as genero,
l.clave as numlinea,
l.Descripcion as nomlinea,
cv.estilo,
cv.colord,
cv.preaut,
cv.fecha,
cf.desc
from costovaria cv
join lineas l on l.clave = cv.linea
join listadeprecios lp on lp.Lista = cv.lista
join costofijo cf on cf.lista = cv.lista
where cv.lista  = $P{lista}
order by cv.linea, cv.estilo asc]]>
	</queryString>
