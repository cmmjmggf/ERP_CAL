	<queryString>
		<![CDATA[select
E.Numero, E.PrimerNombre, E.SegundoNombre, E.Paterno, E.Materno, E.Direccion, E.Colonia, E.Ciudad, E.RFC, str_to_date(E.fechaingreso,'%Y-%m-%d') as FechaIngreso, E.CP,
cast(ifnull(D.Clave,'999')as signed) as numdepto,
ifnull(D.Descripcion,'NO EXISTE DEPTO') as nomdepto
from empleados E
left join departamentos D on D.Clave = E.DepartamentoFisico
where str_to_date(E.fechaingreso,'%Y-%m-%d')
between str_to_date($P{fechaIni},'%d/%m/%Y')
and str_to_date($P{fechaFin},'%d/%m/%Y')
and E.altabaja = 1
order by numdepto asc,  E.Numero asc]]>
