	<parameter name="fechaFin" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
cast(D.Clave as signed) as numdep,
D.Descripcion as nomdep,
E.numero,
E.Busqueda,
date_format(E.FechaIngreso,'%d/%m/%Y') as FechaIngreso,
case
when E.Egreso = '1899-12-30 00:00:00'
then 'ACTIVO'
else
date_format(str_to_date(E.Egreso,'%Y-%m-%d'),'%d/%m/%Y')
end as FechaEgreso
FROM empleados E
join departamentos D on D.Clave =  E.DepartamentoFisico
where str_to_date(E.Egreso,'%Y-%m-%d')
BETWEEN str_to_date($P{fechaIni},'%d/%m/%Y') and str_to_date($P{fechaFin},'%d/%m/%Y')
AND E.AltaBaja = 2
order by numdep ASC,  E.Busqueda ASC]]>
	</queryString>
