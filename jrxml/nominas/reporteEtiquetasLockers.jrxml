	<queryString>
		<![CDATA[SELECT
E.Numero,
CONCAT(E.PrimerNombre,' ',E.SegundoNombre,' ',E.Paterno,' ', E.Materno) AS NOMBRE_COMPLETO,
D.Descripcion AS DEPARTAMENTO
FROM empleados E
join departamentos AS D on D.Clave = E.DepartamentoFisico
where E.DepartamentoFisico = $P{depto}
and E.Altabaja = 1
order by E.Numero asc]]>
	</queryString>
