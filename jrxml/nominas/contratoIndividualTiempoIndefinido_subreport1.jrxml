	<parameter name="empleado" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
    case
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '01' then 'ENERO'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '02' then 'FEBRERO'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '03' then 'MARZO'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '04' then 'ABRIL'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '05' then 'MAYO'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '06' then 'JUNIO'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '07' then 'JULIO'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '08' then 'AGOSTO'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '09' then 'SEPTIEMBRE'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '10' then 'OCUTBRE'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '11' then 'NOVIEMBRE'
    when date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%m') = '12' then 'DICIEMBRE'
    END AS mesIngreso,
    date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%d')  as diaIngreso,
    date_format(str_to_date(E.FechaIMSS,'%Y-%m-%d'),'%Y')  as anoIngreso
FROM empleados E
WHERE NUMERO = $P{empleado};]]>
	</queryString>
	<field name="mesIngreso" class="java.lang.String"/>
	<field name="diaIngreso" class="java.lang.String"/>
	<field name="anoIngreso" class="java.lang.String"/>
