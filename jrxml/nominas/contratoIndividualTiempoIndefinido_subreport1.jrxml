<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="contratoIndividualTiempoIndefinido_subreport1" language="groovy" pageWidth="385" pageHeight="690" columnWidth="385" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="76cc0217-757b-47cc-9699-61c3c5f3b155">
