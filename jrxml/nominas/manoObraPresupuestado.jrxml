	<queryString>
		<![CDATA[SELECT A.semana, A.DEPTO, SUM(A.totalMO) AS totaldepto, A.paressem
FROM
(
SELECT PE.semana, PE.estilo, PE.Pares, FE.`Fraccion`, FE.`CostoMO`,  FE.`CostoMO` * SUM(PE.pares) AS totalMO,
(SELECT SUM(pares) FROM pedidox WHERE ano = $P{ano} AND semana = $P{sem} AND maquila = 1 AND stsavan <> 14 ) as paressem,
CONCAT(F.departamento, ' ',D.descripcion) AS DEPTO
FROM pedidox PE
JOIN fraccionesxestilo FE ON FE.`Estilo` = PE.`Estilo`
JOIN fracciones F ON F.Clave = FE.Fraccion
JOIN departamentos D ON D.Clave =  F.Departamento
WHERE PE.ano = $P{ano}
AND PE.maquila = 1
AND PE.semana = $P{sem}
AND PE.stsavan <> 14
AND FE.afectacostovta = 1
GROUP BY PE.semana, PE.estilo, FE.`Fraccion`
ORDER BY ABS(PE.semana) ASC, PE.estilo, ABS(F.departamento) ASC, ABS(FE.`Fraccion`) ASC
) AS A
GROUP BY A.semana, A.DEPTO
ORDER BY ABS(A.semana),ABS(A.DEPTO) ASC]]>
	</queryString>
