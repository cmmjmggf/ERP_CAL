	<queryString>
		<![CDATA[SELECT  E.Linea, P.Estilo, P.Color, P.ColorT, P.Control, CAST(P.Pares AS SIGNED) AS PARES,
ETX.`cortep`,ETX.`cortef`, (ETX.`cortep`+ETX.`cortef`) AS TCORTE,
ETX.`rayado`,ETX.`rebaja`,
ETX.`folead`,ETX.`entrete`,
ETX.`pespu`,ETX.`ensuel`,
ETX.`prepes`,ETX.`tejido`,
ETX.`montado`,ETX.`adorno`,

(ETX.`cortep` + ETX.`cortef` +
ETX.`rayado` + ETX.`rebaja` +
ETX.`folead` + ETX.`entrete` +
ETX.`pespu` + ETX.`ensuel` +
ETX.`prepes` + ETX.`tejido` +
ETX.`montado` + ETX.`adorno`) AS TDEPTOS,

(ETX.`cortep` + ETX.`cortef` +
ETX.`rayado` + ETX.`rebaja` +
ETX.`folead` + ETX.`entrete` +
ETX.`pespu` + ETX.`ensuel` +
ETX.`prepes` + ETX.`tejido` +
ETX.`montado` + ETX.`adorno`) * CAST(P.Pares AS SIGNED) AS TPARES
FROM pedidox AS P
INNER JOIN estilos AS E ON P.Estilo = E.Clave
INNER JOIN colores AS C ON P.Color = C.Clave AND P.Estilo = C.Estilo
LEFT JOIN estilostiempox AS ETX ON E.Clave = ETX.estilo
WHERE P.Maquila = $P{MAQUILA} AND P.Semana = $P{SEMANA}
AND P.Ano = $P{ANO} AND P.Estatus IN ('A','F') AND P.Control <> '' AND P.Control IS NOT NULL;]]>
	</queryString>
