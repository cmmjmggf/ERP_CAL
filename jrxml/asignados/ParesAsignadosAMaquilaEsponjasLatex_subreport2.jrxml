	</parameter>
	<queryString>
		<![CDATA[SELECT L.Clave AS Linea,E.Clave AS Estilo, sum(P.Pares) AS Pares FROM  pedidox as P
INNER JOIN estilos AS E ON P.Estilo = E.Clave
INNER JOIN lineas AS L ON E.Linea = L.Clave
WHERE L.Clave = $P{Linea}
AND P.Maquila between $P{MAQUILAINICIO} AND $P{MAQUILAFIN} AND P.Ano = $P{ANO}
AND P.Semana BETWEEN $P{SEMANAINICIO} AND $P{SEMANAFIN}
GROUP BY E.Clave]]>
	</queryString>
