	<queryString>
		<![CDATA[SELECT P.Control, P.Estilo, P.Cliente AS Cliente, C.RazonS AS ClienteT,
cast(P.Pares as signed) as Pares, P.Color AS Color,
CO.Descripcion AS ColorT,
CAST(P.Pares AS SIGNED) AS ParesCount
FROM pedidox as P
INNER JOIN clientes AS C ON P.Cliente = C.Clave
INNER JOIN series S ON P.Serie = S.Clave
INNER JOIN colores AS CO ON  CO.Clave = P.Color AND CO.Estilo = P.Estilo
WHERE P.Maquila BETWEEN $P{MAQUILAINICIO} AND $P{MAQUILAFIN} AND P.Ano = $P{ANO} AND P.Semana BETWEEN $P{SEMANAINICIO} AND $P{SEMANAFIN} AND P.stsavan NOT IN(13,14)
ORDER BY ABS(P.Control) ASC;]]>
	</queryString>
