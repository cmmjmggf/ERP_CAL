	<queryString>
		<![CDATA[SELECT
(
CASE
WHEN E.Herramental = 1 THEN "PATRÃ“N BASE"
WHEN E.Herramental = 2 THEN "CARTON Y TESEO"
WHEN E.Herramental = 3 THEN "TESEO"
WHEN E.Herramental = 4 THEN "SUAJE"
END
) AS HERRAMENTAL,
E.Clave AS CLAVE, E.Descripcion AS ESTILOT, SUM(P.Pares) AS PARES,
CAST(P.Pares AS SIGNED) AS PAREX
FROM pedidox AS P
INNER JOIN Estilos AS E ON P.Estilo = E.Clave
WHERE P.Maquila between $P{MAQUILAINICIO} AND $P{MAQUILAFIN} AND P.Ano = $P{ANO}
AND P.Semana BETWEEN $P{SEMANAINICIO} AND $P{SEMANAFIN}
AND E.Herramental IS NOT NULL AND P.Estatus NOT IN ('F')
GROUP BY E.Herramental, E.Clave
ORDER BY E.Herramental DESC]]>
	</queryString>
