	<parameter name="SEMANA_FINAL" class="java.lang.String"/>
	<parameter name="ANO" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT MQ, MQD, Control, CLIENTET, Estilo, Color, ColorT, CAST(Pares AS SIGNED) as Pares, stsavan, FechaPedido, FechaRecepcion, FechaEntrega, AvanceT, ParesSum
FROM ((SELECT
E.MaqPlant1 AS MQ, (SELECT MPL.Descripcion FROM maquilasplantillas AS MPL WHERE MPL.Clave = E.MaqPlant1) AS MQD,
 P.Control,  CT.RazonS AS CLIENTET, P.Estilo, P.Color, P.ColorT,  P.Pares, P.stsavan, P.FechaPedido, P.FechaRecepcion, P.FechaEntrega,
(CASE
WHEN P.stsavan = 1 THEN "PROGRAMADO" WHEN P.stsavan = 2 THEN "CORTE" WHEN P.stsavan = 3 THEN "RAYADO" WHEN P.stsavan = 33 THEN "REBAJADO"
WHEN P.stsavan = 4 THEN "FOLEADO" WHEN P.stsavan = 40 THEN "ENTRETELADO" WHEN P.stsavan = 42 THEN "MAQUILA" WHEN P.stsavan = 44 THEN "ALMACEN CORTE"
WHEN P.stsavan = 5 THEN "PESPUNTE" WHEN P.stsavan = 55 THEN "ENSUELADO" WHEN P.stsavan = 6 THEN "ALMACEN PESPUNTE" WHEN P.stsavan = 7 THEN "TEJIDO"
WHEN P.stsavan = 8 THEN "ALMACEN TEJIDO" WHEN P.stsavan = 9 THEN "MONTADO" WHEN P.stsavan = 10 THEN "ADORNO" WHEN P.stsavan = 11 THEN "ALMACEN ADORNO"
WHEN P.stsavan = 12 THEN "PRODUCTO TERMINADO" WHEN P.stsavan = 13 THEN "FACTURADO" END) AS AvanceT, CAST(P.Pares AS SIGNED) AS ParesSum
FROM pedidox AS P
INNER JOIN clientes AS CT ON P.Cliente = CT.Clave INNER JOIN estilos AS E ON  P.Estilo = E.Clave
WHERE P.Maquila BETWEEN $P{MAQUILA_INICIAL} AND $P{MAQUILA_FINAL} AND P.Semana BETWEEN $P{SEMANA_INICIAL} AND $P{SEMANA_FINAL} AND P.Ano = $P{ANO}  AND E.MaqPlant1 IS NOT NULL AND E.MaqPlant1 NOT IN(0))
UNION

(SELECT
E.MaqPlant2 AS MQ, (SELECT MPL.Descripcion FROM maquilasplantillas AS MPL WHERE MPL.Clave = E.MaqPlant2) AS MQD,
 P.Control,  CT.RazonS AS CLIENTET, P.Estilo, P.Color, P.ColorT,  P.Pares, P.stsavan, P.FechaPedido, P.FechaRecepcion, P.FechaEntrega,
(CASE
WHEN P.stsavan = 1 THEN "PROGRAMADO" WHEN P.stsavan = 2 THEN "CORTE" WHEN P.stsavan = 3 THEN "RAYADO" WHEN P.stsavan = 33 THEN "REBAJADO"
WHEN P.stsavan = 4 THEN "FOLEADO" WHEN P.stsavan = 40 THEN "ENTRETELADO" WHEN P.stsavan = 42 THEN "MAQUILA" WHEN P.stsavan = 44 THEN "ALMACEN CORTE"
WHEN P.stsavan = 5 THEN "PESPUNTE" WHEN P.stsavan = 55 THEN "ENSUELADO" WHEN P.stsavan = 6 THEN "ALMACEN PESPUNTE" WHEN P.stsavan = 7 THEN "TEJIDO"
WHEN P.stsavan = 8 THEN "ALMACEN TEJIDO" WHEN P.stsavan = 9 THEN "MONTADO" WHEN P.stsavan = 10 THEN "ADORNO" WHEN P.stsavan = 11 THEN "ALMACEN ADORNO"
WHEN P.stsavan = 12 THEN "PRODUCTO TERMINADO" WHEN P.stsavan = 13 THEN "FACTURADO" END) AS AvanceT, CAST(P.Pares AS SIGNED) AS ParesSum
FROM pedidox AS P
INNER JOIN clientes AS CT ON P.Cliente = CT.Clave INNER JOIN estilos AS E ON  P.Estilo = E.Clave
WHERE P.Maquila BETWEEN $P{MAQUILA_INICIAL} AND $P{MAQUILA_FINAL} AND P.Semana BETWEEN $P{SEMANA_INICIAL} AND $P{SEMANA_FINAL} AND P.Ano = $P{ANO}  AND E.MaqPlant2 IS NOT NULL AND E.MaqPlant2 NOT IN(0))
UNION

(SELECT
E.MaqPlant3 AS MQ, (SELECT MPL.Descripcion FROM maquilasplantillas AS MPL WHERE MPL.Clave = E.MaqPlant3) AS MQD,
 P.Control,  CT.RazonS AS CLIENTET, P.Estilo, P.Color, P.ColorT,  CAST(P.Pares AS SIGNED) AS Pares, P.stsavan, P.FechaPedido, P.FechaRecepcion, P.FechaEntrega,
(CASE
WHEN P.stsavan = 1 THEN "PROGRAMADO" WHEN P.stsavan = 2 THEN "CORTE" WHEN P.stsavan = 3 THEN "RAYADO" WHEN P.stsavan = 33 THEN "REBAJADO"
WHEN P.stsavan = 4 THEN "FOLEADO" WHEN P.stsavan = 40 THEN "ENTRETELADO" WHEN P.stsavan = 42 THEN "MAQUILA" WHEN P.stsavan = 44 THEN "ALMACEN CORTE"
WHEN P.stsavan = 5 THEN "PESPUNTE" WHEN P.stsavan = 55 THEN "ENSUELADO" WHEN P.stsavan = 6 THEN "ALMACEN PESPUNTE" WHEN P.stsavan = 7 THEN "TEJIDO"
WHEN P.stsavan = 8 THEN "ALMACEN TEJIDO" WHEN P.stsavan = 9 THEN "MONTADO" WHEN P.stsavan = 10 THEN "ADORNO" WHEN P.stsavan = 11 THEN "ALMACEN ADORNO"
WHEN P.stsavan = 12 THEN "PRODUCTO TERMINADO" WHEN P.stsavan = 13 THEN "FACTURADO" END) AS AvanceT, CAST(P.Pares AS SIGNED) AS ParesSum
FROM pedidox AS P
INNER JOIN clientes AS CT ON P.Cliente = CT.Clave INNER JOIN estilos AS E ON  P.Estilo = E.Clave
WHERE P.Maquila BETWEEN $P{MAQUILA_INICIAL} AND $P{MAQUILA_FINAL} AND P.Semana BETWEEN $P{SEMANA_INICIAL} AND $P{SEMANA_FINAL} AND P.Ano = $P{ANO} AND E.MaqPlant3 IS NOT NULL AND E.MaqPlant3 NOT IN(0))
UNION ALL
(SELECT
E.MaqPlant4 AS MQ, (SELECT MPL.Descripcion FROM maquilasplantillas AS MPL WHERE MPL.Clave = E.MaqPlant4) AS MQD,
 P.Control,  CT.RazonS AS CLIENTET, P.Estilo, P.Color, P.ColorT,  CAST(P.Pares AS SIGNED), P.stsavan, P.FechaPedido, P.FechaRecepcion, P.FechaEntrega,
(CASE
WHEN P.stsavan = 1 THEN "PROGRAMADO" WHEN P.stsavan = 2 THEN "CORTE" WHEN P.stsavan = 3 THEN "RAYADO" WHEN P.stsavan = 33 THEN "REBAJADO"
WHEN P.stsavan = 4 THEN "FOLEADO" WHEN P.stsavan = 40 THEN "ENTRETELADO" WHEN P.stsavan = 42 THEN "MAQUILA" WHEN P.stsavan = 44 THEN "ALMACEN CORTE"
WHEN P.stsavan = 5 THEN "PESPUNTE" WHEN P.stsavan = 55 THEN "ENSUELADO" WHEN P.stsavan = 6 THEN "ALMACEN PESPUNTE" WHEN P.stsavan = 7 THEN "TEJIDO"
WHEN P.stsavan = 8 THEN "ALMACEN TEJIDO" WHEN P.stsavan = 9 THEN "MONTADO" WHEN P.stsavan = 10 THEN "ADORNO" WHEN P.stsavan = 11 THEN "ALMACEN ADORNO"
WHEN P.stsavan = 12 THEN "PRODUCTO TERMINADO" WHEN P.stsavan = 13 THEN "FACTURADO" END) AS AvanceT, CAST(P.Pares AS SIGNED) AS ParesSum
FROM pedidox AS P
INNER JOIN clientes AS CT ON P.Cliente = CT.Clave INNER JOIN estilos AS E ON  P.Estilo = E.Clave
WHERE  P.Maquila BETWEEN $P{MAQUILA_INICIAL} AND $P{MAQUILA_FINAL} AND P.stsavan NOT IN(13,14)  AND P.Semana BETWEEN $P{SEMANA_INICIAL} AND $P{SEMANA_FINAL} AND P.Ano = $P{ANO}
AND E.MaqPlant4 IS NOT NULL AND E.MaqPlant4 NOT IN(0))
) AS A ORDER BY ABS(A.MQ) ASC, A.Estilo ASC;]]>
