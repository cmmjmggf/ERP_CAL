	<queryString>
		<![CDATA[SELECT A.Linea, A.Estilo, A.Total,
FORMAT(SUM(CASE WHEN B.Departamento = 10 THEN B.Tiempo ELSE NULL END),2) AS CORTE_PIEL,
FORMAT(SUM(CASE WHEN B.Departamento = 10 THEN B.Tiempo ELSE NULL END),2) AS CORTE_FORRO,
FORMAT(SUM(CASE WHEN B.Departamento = 10 THEN B.Tiempo ELSE NULL END),2) AS TOTAL_CORTE,
FORMAT(SUM(CASE WHEN B.Departamento = 20 THEN B.Tiempo ELSE NULL END),2) AS RAYADO,
FORMAT(SUM(CASE WHEN B.Departamento = 30 THEN B.Tiempo ELSE NULL END),2) AS REBAJADO,
FORMAT(SUM(CASE WHEN B.Departamento = 40 THEN B.Tiempo ELSE NULL END),2) AS FOLEADO,
FORMAT(SUM(CASE WHEN B.Departamento = 90 THEN B.Tiempo ELSE NULL END),2) AS ENTRETELA,
FORMAT(SUM(CASE WHEN B.Departamento = 110 THEN B.Tiempo ELSE NULL END),2) AS PESPUNT,
FORMAT(SUM(CASE WHEN B.Departamento = 140 THEN B.Tiempo ELSE NULL END),2) AS ENSUELADO,
FORMAT(SUM(CASE WHEN B.Departamento = 120 THEN B.Tiempo ELSE NULL END),2) AS "PRE-PESP",
FORMAT(SUM(CASE WHEN B.Departamento = 60 THEN B.Tiempo ELSE NULL END),2) AS LASER,
FORMAT(SUM(CASE WHEN B.Departamento IN(180,190) THEN B.Tiempo ELSE NULL END),2) AS MONTADO,
FORMAT(SUM(CASE WHEN B.Departamento IN(210,220) THEN B.Tiempo ELSE NULL END),2) AS ADORNO,
FORMAT(SUM(CASE WHEN B.Departamento IN(10,20,30,40,90,110,120,140,60,180,190,210,220) THEN B.Tiempo ELSE NULL END),2) AS TOTALES,
FORMAT((SUM(CASE WHEN B.Departamento IN(10,20,30,40,90,110,120,140,60,180,190,210,220) THEN B.Tiempo ELSE NULL END) * $P{PARES}),2) AS TOTALESXPARES
FROM tiemposxestilodepto AS A
INNER JOIN tiemposxestilodepto_has_deptos AS B
ON A.ID = B.TiempoXEstiloDepto
INNER JOIN departamentos AS D
ON D.Clave = B.Departamento
WHERE A.Linea = $P{LINEA} AND A.Estilo = $P{ESTILO};]]>
	</queryString>
