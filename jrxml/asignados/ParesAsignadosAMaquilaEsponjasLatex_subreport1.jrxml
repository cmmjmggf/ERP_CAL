	<queryString language="SQL">
		<![CDATA[SELECT
     sum(P.Pares) AS Pares,
     L.`Clave` AS Linea,
     E.`Clave` AS Estilo
FROM
     `pedidox` P INNER JOIN `estilos` E ON P.`Estilo` = E.`Clave`
     INNER JOIN `lineas` L ON E.`Linea` = L.`Clave`
WHERE
     L.Clave = 1860
 AND P.Maquila BETWEEN 1 AND 1
 AND P.Ano = 2019
 AND P.Semana BETWEEN 15 AND 15]]>
	</queryString>
