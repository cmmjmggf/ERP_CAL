	<queryString>
		<![CDATA[SELECT
ifnull(em.numero,9999) AS NumEmpleado,
ifnull(concat(em.numero,' ', em.Busqueda),'9999 NO EXISTE EMPLEADO') AS NomEmp,
fpn.control,
fpn.semana,
fpn.fecha,
fpn.estilo,
fpn.pares,
concat(fpn.numfrac,' ',fpn.fraccion) as nombreFracc,
fpn.preciofrac,
fpn.preciofrac * fpn.pares as total
FROM fracpagnomina fpn
LEFT JOIN empleados em on em.Numero = fpn.numeroempleado
where fpn.numfrac = $P{fraccion} and fpn.anio = $P{ano} and fpn.semana = $P{sem} and fpn.maquila = $P{maq}
order by NumEmpleado asc, fpn.fecha asc, fpn.control asc]]>
	</queryString>
