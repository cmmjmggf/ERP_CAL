	<queryString>
		<![CDATA[SELECT
CT.control,
ODP.Estilo,
ODP.Color,
ODP.ColorT,
CT.Fecha,
CAST(ODP.Pares AS DECIMAL(8,0)) AS Pares,
CT.Defecto,
CT.Detalle,
D.Descripcion AS DefNombre,
DD.Descripcion AS DetDefNombre,
ODP.Semana
FROM controlcali CT
INNER JOIN ordendeproduccion ODP ON ODP.ControlT =  CT.Control
INNER JOIN defectosdetalle DD ON CT.Detalle = DD.Clave
INNER JOIN defectos D ON CT.Defecto = D.Clave
WHERE ODP.Maquila = $P{maq} AND CT.Docto = $P{docto}
ORDER BY ODP.Semana ASC, CT.Control ASC]]>
	</queryString>
