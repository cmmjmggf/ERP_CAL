	<queryString>
		<![CDATA[SELECT
CAST(M.Clave AS SIGNED) AS ClaveMaq,
M.Nombre AS Maquila,
P.Control, P.Fecha,
P.Estilo,
P.Color, P.Talla, E.Descripcion AS NombreEstilo,
CASE WHEN P.ParteZapato = '3' THEN 'PAR'
WHEN P.ParteZapato = '1' THEN 'IZQ'
WHEN P.ParteZapato = '2' THEN 'DER' END ParteZapato,
CAST(P.Pares AS DECIMAL(4,2)) AS Pares,
L.Clave AS ClaveLinea,
L.Descripcion AS NombreLinea,
A.Clave AS ClaveArt,
A.Descripcion AS NombreArt,
PI.Clave AS ClavePieza,
PI.Descripcion AS NombrePieza,
P.Fraccion,
P.Material,
D.Clave AS ClaveDefecto,
D.Descripcion AS NombreDefecto,
DD.Clave AS ClaveDefectoDet,
DD.Descripcion AS NombreDefectoDet,
CASE WHEN P.TipoCargo = '1' THEN 'C/Cgo'
WHEN P.TipoCargo = '2' THEN 'S/Cgo'
END AS TipoCargo,
P.Consumo,
PM.Precio,
CASE WHEN P.TipoCargo = '1' THEN
CAST(PM.Precio*P.Consumo AS DECIMAL(6,2))
ELSE 0 END AS Costo,
P.Usuario
from piochas P
INNER JOIN maquilas M ON M.Clave = CAST(P.Maq AS SIGNED)
INNER JOIN articulos A ON A.Clave = P.Material
INNER JOIN estilos E ON E.Clave = P.Estilo
INNER JOIN lineas L ON L.Clave = E.Linea
INNER JOIN piezas PI ON PI.Clave = P.Pieza
INNER JOIN defectos D ON D.Clave = P.Defecto
INNER JOIN defectosdetalle DD ON DD.Clave = P.Detalle
INNER JOIN preciosmaquilas PM ON PM.Articulo = P.Material AND PM.Maquila = '1'
WHERE P.Estatus = '1'
ORDER BY Estilo, P.Control ASC]]>
	</queryString>
