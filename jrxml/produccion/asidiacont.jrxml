	<queryString language="SQL">
		<![CDATA[SELECT
     empleados.`Numero` AS empleados_Numero,
     programacion.`numemp` AS programacion_numemp,
     empleados.`PrimerNombre` AS empleados_PrimerNombre,
     empleados.`SegundoNombre` AS empleados_SegundoNombre,
     pedidox.`Control` AS pedidox_Control,
     programacion.`control` AS programacion_control,
     lineas.`Descripcion` AS lineas_Descripcion,
     lineas.`Clave` AS lineas_Clave,
     series.`Clave` AS series_Clave,
     estilos.`Clave` AS estilos_Clave,
     estilos.`Descripcion` AS estilos_Descripcion,
     programacion.`frac` AS programacion_frac,
     programacion.`estilo` AS programacion_estilo,
     estilos.`Horma` AS estilos_Horma,
     pedidox.`Pares` AS pedidox_Pares
FROM
     `programacion` programacion,
     `empleados` empleados,
     `pedidox` pedidox,
     `colores` colores,
     `estilos` estilos,
     `lineas` lineas,
     `series` series
WHERE
     programacion.`numemp` = empleados.`Numero`
 AND programacion.`control` = pedidox.`Control`
 AND pedidox.`Color` = colores.`Clave`
 AND pedidox.`Estilo` = colores.`Estilo`
 AND colores.`Estilo` = estilos.`Clave`
 AND estilos.`Linea` = lineas.`Clave`
 AND pedidox.`Serie` = series.`Clave`]]>
