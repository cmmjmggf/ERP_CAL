	<parameter name="depto" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select sum(A.pares) AS total_pares,

(SELECT sum(fpn2.subtot) as total_pesos
from fracpagnomina	fpn2
where fpn2.anio = 2019
and fpn2.semana = 15
and fpn2.depto = $P{depto}
and date_format(fpn2.fecha,'%d/%m/%Y') = '04/04/2019') as total_pesos

from (SELECT DISTINCT(fpn.control), fpn.pares
from fracpagnomina	fpn
where fpn.anio = 2019
and fpn.semana = 15
and fpn.depto = $P{depto}
and date_format(fpn.fecha,'%d/%m/%Y') = '04/04/2019'
order by fpn.control) AS A]]>
