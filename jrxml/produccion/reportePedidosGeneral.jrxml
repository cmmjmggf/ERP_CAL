	<queryString>
		<![CDATA[select
P.Clave, P.Control, P.cliente as ClaveCliente,
(select razons from clientes where clave = P.cliente) as NombreCliente,
P.FechaEntrega,
(CASE
WHEN P.stsavan = 0 THEN 0
WHEN P.stsavan = 1 THEN 1
WHEN P.stsavan = 2 THEN 2
WHEN P.stsavan = 3 THEN 3
WHEN P.stsavan = 33 THEN 4
WHEN P.stsavan = 4 THEN 5
WHEN P.stsavan = 40 THEN 6
WHEN P.stsavan = 42 THEN 7
WHEN P.stsavan = 44 THEN 8
WHEN P.stsavan = 5 THEN 9
WHEN P.stsavan = 55 THEN 10
WHEN P.stsavan = 6 THEN 11
WHEN P.stsavan = 7 THEN 12
WHEN P.stsavan = 8 THEN 13
WHEN P.stsavan = 9 THEN 14
WHEN P.stsavan = 10 THEN 15
WHEN P.stsavan = 11 THEN 16
WHEN P.stsavan = 12 THEN 17
WHEN P.stsavan = 13 THEN 18
END)  AS stsavan,
(case
when (P.stsavan  = 0) then 'Pre-programado'
when (P.stsavan  = 1) then 'Programado'
when (P.stsavan = 2) then 'Corte'
when (P.stsavan = 3) then 'Rayado'
when (P.stsavan = 33) then 'Rebajado'
when (P.stsavan = 4) then 'Foleado'
when (P.stsavan = 40) then 'Entretelado'
when (P.stsavan = 42) then 'Proceso Maq'
when (P.stsavan = 44) then 'Alm-Corte'
when (P.stsavan = 5) then 'Pespunte'
when (P.stsavan = 55) then 'Ensuelado'
when (P.stsavan = 6) then 'Alm-Pespu'
when (P.stsavan = 7) then 'Tejido'
when (P.stsavan = 8) then 'Alm-Tejido'
when (P.stsavan = 9) then 'Montado'
when (P.stsavan = 10) then 'Adorno'
when (P.stsavan = 11) then 'Alm-Adorno'
when (P.stsavan = 12) then 'Terminado'
when (P.stsavan = 13) then 'Facturado'
end) AS EstatusProduccion,
P.stsavan as Avance,
cast(P.Pares as decimal(6,2)) as Pares,
P.Estilo AS Estilo,
P.ColorT AS ColorT
from pedidox P
join clientesprioridad CP on P.Cliente = CP.Cliente
where P.stsavan not in (0,13,14) and P.maquila  < 98
and P.Semana BETWEEN $P{dSem} AND $P{aSem}
AND P.Ano = $P{Ano}
order by ClaveCliente ASC, abs(stsavan) ASC,  P.Control ASC;]]>
