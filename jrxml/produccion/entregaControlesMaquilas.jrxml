	<queryString>
		<![CDATA[SELECT AV.contped, AV.fec6, PE.Estilo, PE.Color, PE.ColorT, LP.PrecioVta,
cast(PE.Pares as signed) as Pares,
LP.PrecioVta * PE.Pares as Total
FROM avaprd AV
join pedidox PE on PE.Control = AV.contped
join listapreciosmaquilas LP on LP.Estilo = PE.Estilo AND LP.Color = PE.Color AND LP.Maq = PE.Maquila
where almpesp = 901310710
order by AV.contped ASC;]]>
	</queryString>
