	<queryString>
		<![CDATA[select
EDO.Descripcion AS Estado,
CT.Ciudad,
cast(CT.Clave as signed) AS Cliente,
CT.RazonS AS ClienteT,
PE.Control,
PE.Clave,
str_to_date(PE.FechaEntrega,'%d/%m/%Y') as FechaEntrega,
str_to_date(PE.FechaPedido,'%d/%m/%Y') as FechaPedido,
str_to_date(PE.FechaRecepcion,'%d/%m/%Y') as FechaRecepcion,
PE.Estilo,
PE.Color,
PE.ColorT,
PE.Maquila,
PE.Semana,
CASE
WHEN PE.Control = 0 OR PE.Control IS NULL THEN 0
WHEN PE.Control > 0 AND C.EstatusProduccion IS NULL THEN 1
WHEN PE.Control > 0 AND C.EstatusProduccion IS NOT NULL THEN CAST(D.Clave AS SIGNED)
END AS Avance,
CASE
WHEN PE.Control = 0 OR PE.Control IS NULL THEN 'PRE-PROGRAMADO'
WHEN PE.Control > 0 AND C.EstatusProduccion IS NULL THEN 'PROGRAMADO'
WHEN PE.Control > 0 AND C.EstatusProduccion IS NOT NULL THEN D.Descripcion
END AS AvanceT,
L.Clave AS Linea,
L.Descripcion AS LineaT,
cast(PE.Pares as decimal(4,0)) as Pares
from pedidox PE
left join controles C on PE.Control =  C.control
left join departamentos D on D.Descripcion  = C. EstatusProduccion
join clientes CT on CT.Clave = PE.Cliente
join estados EDO on EDO.Clave = CT.Estado
join estilos E on E.Clave = PE.Estilo
join lineas L on L.clave = E.Linea
where PE.maquila not in('98')
and PE.estatus in ('A')
and PE.agente = $P{agente}
order by Estado ASC, Cliente ASC, PE.FechaEntrega ASC, PE.Clave ASC, PE.Estilo ASC, PE.Color ASC  ;]]>
	</queryString>
