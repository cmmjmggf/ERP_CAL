	<queryString>
		<![CDATA[select
pe.maquila as maq,
ct.sem,
ct.docto,
pe.Cliente,
(select razons from clientes where clave = pe.cliente) as NombreCliente,
ct.control,
ct.estilo,
ct.color,
pe.ColorT,
ct.fecha,
str_to_date(pe.FechaEntrega,'%d/%m/%Y') as FechaEntrega,
datediff(str_to_date(pe.FechaEntrega,'%d/%m/%Y'),CT.fecha) as Dias,
ct.pares,
ifnull((select PrecioVta from listapreciosmaquilas PM where PM.Estilo = C.Estilo AND PM.Color = C.Clave AND PM.Maq = pe.maquila limit 1 ),0) as prevta,
(ct.pares * (ifnull((select PrecioVta from listapreciosmaquilas PM where PM.Estilo = C.Estilo AND PM.Color = C.Clave AND PM.Maq = pe.maquila limit 1 ),0))) as subtotal_sintej,
ifnull((select CAST(FXE.CostoMO AS DECIMAL(10,2)) from fraccionesxestilo FXE where FXE.Estilo = C.Estilo AND FXE.Fraccion = '401'),0) as tejida,
ifnull((select CAST(FXE.CostoMO AS DECIMAL(10,2)) from fraccionesxestilo FXE where FXE.Estilo = C.Estilo AND FXE.Fraccion = '401'),0) * CT.pares as total_tej,
(ifnull((select CAST(FXE.CostoMO AS DECIMAL(10,2)) from fraccionesxestilo FXE where FXE.Estilo = C.Estilo AND FXE.Fraccion = '401'),0) * CT.pares)
+
(CT.pares * (ifnull((select PrecioVta from listapreciosmaquilas PM where PM.Estilo = C.Estilo AND PM.Color = C.Clave AND PM.Maq = pe.maquila limit 1 ),0))) as subtotal_gen

from controlterm ct
join pedidox pe on pe.control = ct.control
left join colores C on C.Clave = ct.color and C.estilo = ct.estilo
where ct.fecha between str_to_date($P{fechaIni},'%d/%m/%Y') and str_to_date($P{fechaFin},'%d/%m/%Y')
order by abs(pe.maquila) asc, abs(ct.docto) asc, abs(ct.sem) asc, CT.control;]]>
	</queryString>
