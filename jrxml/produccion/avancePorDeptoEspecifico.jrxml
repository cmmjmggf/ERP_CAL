	<queryString>
		<![CDATA[SELECT
case
when  (D.Clave = '150') then
ifnull((
select EM.Busqueda from fracpagnomina FPN
left JOIN empleados EM on EM.Numero = FPN.numeroempleado
where FPN.depto = 150 and FPN.numfrac = 401 and FPN.control = C.Control
limit 1 ),'')

when  (D.Clave = '110') then
ifnull((
SELECT E.Busqueda
FROM fracpagnomina fpn
left join empleados E on E.Numero = fpn.numeroempleado
where fpn.numfrac = 300 and fpn.depto = 110 and fpn.control = C.Control
limit 1 ),'')

else ''
end as Empleado,


C.Pedido, C.Control, str_to_date(PE.FechaProg,'%Y-%m-%d') FechaProgramacion,
cast(ifnull(D.Clave,'1') as signed) as ClaveDepto,
ifnull(D.Descripcion,'PROGRAMADO') as nombreDepto,
case
when (D.Clave is null) then str_to_date(PE.FechaProg,'%Y-%m-%d')
when (D.Clave = '10') then A.fec2
when (D.Clave = '20') then A.fec3
when (D.Clave = '30') then A.fec33
when (D.Clave = '40') then A.fec4
when (D.Clave = '90') then A.fec40
when (D.Clave = '100') then A.fec42
when (D.Clave = '105') then A.fec44
when (D.Clave = '110') then A.fec5
when (D.Clave = '140') then A.fec55
when (D.Clave = '130') then A.fec6
when (D.Clave = '150') then A.fec7
when (D.Clave = '160') then A.fec8
when (D.Clave = '180') then A.fec9
when (D.Clave = '210') then A.fec10
when (D.Clave = '230') then A.fec11
when (D.Clave = '240') then A.fec12
end as fecha,
now() as fecActual,
cast(PE.Pares as decimal(5,2)) as Pares,
cast(ifnull(PE.ParesFacturados,0) as decimal(5,2)) as ParesFacturados,
cast(ifnull(PE.Pares,0) as signed) - cast(ifnull(PE.ParesFacturados,0) as signed) as Saldo,
E.GdoDif,
PE.Estilo
FROM controles C
left join pedidox PE on PE.Control =  C.control
left join departamentos D on D.Descripcion  = C. EstatusProduccion
left join avaprd A on A.contped = C.control
left join estilos E on E.Clave = PE.Estilo
left join programacion PR on PR.control = C.control
where
(D.Clave not in ('260','270') or D.Clave is null)
and
(
cast(PE.maquila as signed) = $P{maq}
and PE.ano = $P{ano}
and D.Clave = $P{depto}
)
order by Empleado,C.control ASC]]>
	</queryString>
