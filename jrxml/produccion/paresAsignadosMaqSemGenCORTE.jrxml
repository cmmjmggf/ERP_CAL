	<parameter name="ano" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
CAST(PE.semana AS SIGNED) AS sem,
SUM(CASE WHEN PE.maquila = 1 THEN PE.pares ELSE 0 END) AS maq1,
(SELECT pecor FROM cierrasem  WHERE sem = PE.semana AND maq =1 AND año = $P{ano} ORDER BY conce DESC LIMIT 1) AS pmaq1,
(SELECT ticor FROM cierrasem  WHERE sem = PE.semana AND maq =1 AND año = $P{ano} ORDER BY conce DESC LIMIT 1) AS minmaq1,
(SELECT micor FROM cierrasem  WHERE sem = PE.semana AND maq =1 AND año = $P{ano} ORDER BY conce DESC LIMIT 1) AS capmaq1
FROM pedidox PE
WHERE PE.stsavan <> 14
AND CAST(PE.ano AS SIGNED) = $P{ano}
GROUP BY PE.semana
ORDER BY sem ASC;]]>
	</queryString>
