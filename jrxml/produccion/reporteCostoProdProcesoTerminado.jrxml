	<queryString>
		<![CDATA[select
PE.Control, PE.Estilo, PE.Color, PE.ColorT,
E.Linea, L.Descripcion AS LineaT,
cast(PE.Pares as decimal(4,0)) as Pares,
cast(ifnull(PE.ParesFacturados,0) as decimal(4,0)) as ParesFacturados,
cast(PE.DeptoProduccion as signed) AS Depto, PE.EstatusProduccion  as DeptoT,

ifnull((SELECT sum(EPMP.costomp) FROM estilosprocesod EPMP where EPMP.estilo = PE.Estilo and EPMP.color = PE.Color and EPMP.maq = PE.maquila  and CAST(EPMP.depto AS SIGNED) <= CAST(PE.DeptoProduccion AS SIGNED)),0) +
ifnull((select sum(EPMO.costo) from estilosprocesodmo EPMO where EPMO.Estilo = PE.Estilo and CAST(EPMO.depto AS SIGNED) <= CAST(PE.DeptoProduccion AS SIGNED)),0) +
ifnull((select sum(GFD.costo) from gastosfabricaxdepto GFD where clave in (
	select  DISTINCT(depto) as depto
	from estilosprocesod
	where estilo = PE.Estilo and color = PE.Color and maq = PE.maquila
	and cast(depto as signed) <= CAST(PE.DeptoProduccion AS SIGNED))),0) as Costo
from pedidox PE
join estilos E on E.Clave = PE.Estilo
join lineas L on L.Clave = E.Linea
where PE.stsavan <> 14 and PE.stsavan <> 13
and PE.stsavan in (2, 10, 11, 2, 3, 33, 4, 40, 44, 5, 55, 6, 7, 8, 9)
and PE.maquila <> '98' and PE.maquila = $P{maq}
order by Depto asc, E.Linea asc, PE.Control asc;]]>
	</queryString>
