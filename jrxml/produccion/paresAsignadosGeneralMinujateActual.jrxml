	<parameter name="semact" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
cast(A.Semana as SIGNED) as Semana, SUM(A.paresped) as paresprog,
A.personal, ROUND(SUM(totalmin),0) AS minutaje, A.personal *  A.MinxSemXEmp AS minutpresupu,
SUM(A.entregados) AS entre

FROM
(
SELECT PE.semana, PE.estilo, SUM(PE.Pares) AS paresped, 2880 AS MinxSemXEmp,
IFNULL((SELECT pepes FROM cierrasem WHERE año = PE.ano AND sem = PE.semana AND maq = 1 ORDER BY conce DESC LIMIT 1),
(SELECT COUNT(*) FROM empleados WHERE AltaBaja = 1 AND DepartamentoFisico = 110)) AS personal,
SUM(PE.Pares) * ET.pespu AS totalmin,
SUM(CASE WHEN ct.maq = 1 THEN ct.pares ELSE 0 END) AS entregados
FROM pedidox PE
LEFT JOIN controlterm ct ON ct.control = PE.control
JOIN estilostiempox ET ON ET.estilo = PE.estilo
WHERE PE.ano = $P{ano}
AND PE.maquila = 1
AND PE.stsavan <> 14
GROUP BY PE.semana, PE.estilo
ORDER BY ABS(PE.semana) ASC, PE.estilo

) AS A
GROUP BY A.semana
ORDER BY ABS(A.semana)]]>
	</queryString>
