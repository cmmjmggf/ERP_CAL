	<queryString>
		<![CDATA[select
FPN.numeroempleado as numemp,
E.Busqueda AS nomemp,
FPN.control,
FPN.semana,
FPN.fecha,
FPN.estilo,
FPN.numfrac,
F.Descripcion as nomfrac,
FPN.preciofrac,
FPN.pares,
FPN.subtot
from fracpagnomina FPN
join empleados E on E.Numero = FPN.numeroempleado
join fracciones F on F.Clave = FPN.numfrac
join pedidox AS P ON P.Control = FPN.control
where FPN.semana = $P{sem} and FPN.anio = $P{ano} and FPN.numeroempleado = $P{emp} AND P.stsavan NOT IN(14)
order by FPN.numfrac asc,FPN.fecha asc,  FPN.ID asc]]>
	</queryString>
