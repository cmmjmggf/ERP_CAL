	<queryString>
		<![CDATA[SELECT  X.EMPLEADO_NOMBRE, SUM(X.PARES) AS PARES, SUM(X.X_CONTROL) AS X_CONTROL, SUM(X.ENTREGADO) AS ENTREGADO, SUM(X.DEVOLUCION) AS DEVOLUCION,
SUM(X.ENT_DEV) AS ENT_DEV, (SUM(X.X_CONTROL)-SUM(X.ENT_DEV)) AS DIFERENCIA, (SUM(X.ENT_DEV) / SUM(X.PARES)) AS CONSUMOPROMXPAR, SUM(X.SISTEMAPESOS) AS SISTEMAPESOS, SUM(X.ENT_DEV * X.Precio) AS REALPESOS_SISTEMA,
(SUM(X.SISTEMAPESOS) - SUM(X.ENT_DEV * X.Precio)) AS DIFERENCIA_PESOS, SUM(X.PIOCHA) AS PIOCHA

FROM (SELECT A.Empleado AS EMPLEADO_CLAVE,

CONCAT(E.Numero," - ",E.PrimerNombre," ", E.SegundoNombre ," ",E.Paterno," ",E.Materno) AS EMPLEADO_NOMBRE,

A.Control AS Control, A.Estilo, A.Color, CAST(A.Pares AS SIGNED) AS PARES,
CAST(A.PrecioActual AS DECIMAL(20,4)) AS Precio,
CAST((SELECT SUM(AX.Devolucion) FROM asignapftsacxc AS AX WHERE AX.Empleado = E.Numero AND AX.Control = A.Control AND AX.Articulo = A.Articulo AND AX.Fraccion = A.Fraccion) AS DECIMAL(20,4)) AS DEVOLUCION,

(((SELECT SUM(OPDD.Cantidad) FROM ordendeproducciond AS OPDD WHERE OPDD.OrdenDeProduccion = OP.ID AND OPDD.Articulo = OPD.Articulo) /OP.Pares)*A.Pares)  AS X_CONTROL,

(SELECT SUM(AX.Abono) FROM asignapftsacxc AS AX
WHERE AX.Control = A.Control AND AX.Articulo = A.Articulo AND AX.Fraccion = A.Fraccion AND AX.Empleado = E.Numero)   AS ENTREGADO,




((SELECT SUM(OPDD.Cantidad) FROM ordendeproducciond AS OPDD WHERE OPDD.OrdenDeProduccion = OP.ID AND OPDD.Articulo = OPD.Articulo) /OP.Pares) AS X_ESTILO,

(SELECT SUM(AX.Abono) FROM asignapftsacxc AS AX
WHERE AX.Control = A.Control AND AX.Articulo = A.Articulo AND AX.Fraccion = A.Fraccion AND AX.Empleado = E.Numero) - CAST((SELECT SUM(AX.Devolucion) FROM asignapftsacxc AS AX WHERE AX.Empleado = E.Numero AND AX.Control = A.Control  AND AX.Articulo = A.Articulo AND AX.Fraccion = A.Fraccion) AS DECIMAL(20,4))
 AS ENT_DEV,


 (((SELECT SUM(OPDD.Cantidad) FROM ordendeproducciond AS OPDD WHERE OPDD.OrdenDeProduccion = OP.ID AND OPDD.Articulo = OPD.Articulo) /OP.Pares)*A.Pares)
 AS DIFERENCIA,

((((SELECT SUM(OPDD.Cantidad) FROM ordendeproducciond AS OPDD WHERE OPDD.OrdenDeProduccion = OP.ID AND OPDD.Articulo = OPD.Articulo) /OP.Pares)*A.Pares)/A.Pares) AS DCM2,

(((SELECT SUM(AX.Abono) FROM asignapftsacxc AS AX WHERE AX.Control = A.Control AND AX.Articulo = A.Articulo AND AX.Fraccion = A.Fraccion) -(SELECT SUM(AX.Devolucion) FROM asignapftsacxc AS AX WHERE AX.Empleado = E.Numero AND AX.Control = A.Control AND AX.Articulo = A.Articulo AND AX.Fraccion = A.Fraccion) ) / (((SELECT SUM(OPDD.Cantidad) FROM ordendeproducciond AS OPDD WHERE OPDD.OrdenDeProduccion = OP.ID AND OPDD.Articulo = OPD.Articulo) /OP.Pares)*A.Pares)) AS PORCENTAJE,

((((SELECT SUM(OPDD.Cantidad) FROM ordendeproducciond AS OPDD WHERE OPDD.OrdenDeProduccion = OP.ID AND OPDD.Articulo = OPD.Articulo) /OP.Pares) * A.Pares) * A.PrecioActual) AS SISTEMAPESOS,

(A.PrecioActual * (IFNULL(A.Abono,0)-(IFNULL(A.Basura,0)+IFNULL((SELECT SUM(AX.Devolucion) FROM asignapftsacxc AS AX WHERE AX.Empleado = E.Numero AND AX.Control = A.Control AND AX.Articulo = A.Articulo AND AX.Fraccion = A.Fraccion),0)))) AS REALPESOS_SISTEMA,

0 AS DIFERENCIAPESOS_SISTEMA,
cast(A.Piocha as decimal(10,4)) AS PIOCHA,

(SELECT CONCAT(OPD.Articulo," ",OPD.ArticuloT) FROM ordendeproduccion AS OP INNER JOIN ordendeproducciond AS OPD ON OP.ID = OPD.OrdenDeProduccion
WHERE OP.ControlT = A.Control AND A.Articulo = OPD.Articulo  AND OP.Ano = $P{ANO} LIMIT 1) AS ARTICULO_DESC

FROM ordendeproduccion AS OP INNER JOIN ordendeproducciond AS OPD ON OP.ID = OPD.OrdenDeProduccion INNER JOIN asignapftsacxc AS A ON
OP.ControlT = A.Control AND A.Articulo = OPD.Articulo INNER JOIN empleados AS E ON A.Empleado = E.Numero
WHERE A.Maquila = $P{MAQUILA} AND A.Semana BETWEEN $P{SEMANAINICIO} AND $P{SEMANAFIN}  AND A.Empleado > 0 AND A.TipoMov = 1 AND A.Fraccion = 100 AND E.AltaBaja = 1 AND OP.Ano = $P{ANO} AND
(CASE WHEN $P{ARTICULO} ="" THEN A.Articulo LIKE '%%' ELSE A.Articulo = $P{ARTICULO} END)
GROUP BY  E.Numero, A.Control, A.Articulo
ORDER BY ABS(A.Empleado) ASC,    A.Estilo ASC, ABS(A.Control) ASC) AS X
GROUP BY X.EMPLEADO_CLAVE;]]>
	</queryString>
