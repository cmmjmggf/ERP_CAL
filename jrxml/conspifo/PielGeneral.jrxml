	<queryString>
		<![CDATA[SELECT A.Empleado AS EMPLEADO_CLAVE, REPLACE((SELECT CONCAT(E.Numero," - ",E.PrimerNombre," ",(CASE WHEN E.SegundoNombre = 0 THEN "" ELSE E.SegundoNombre END)," ",E.Paterno," ",E.Materno) FROM empleados AS E WHERE E.Numero = A.Empleado LIMIT 1),"0","") AS EMPLEADO_NOMBRE,

OP.ControlT AS Control, OP.Estilo, OP.Color, OPD.Articulo, OPD.ArticuloT,
                            A.PrecioActual AS Precio, CAST(OP.Pares AS SIGNED) AS PARES,

((SELECT SUM(OPDD.Cantidad) FROM ordendeproducciond AS OPDD WHERE OPDD.OrdenDeProduccion = OP.ID AND OPDD.Articulo = OPD.Articulo) /OP.Pares) AS Consumo,
ifnull(OP.CantidadPiel1,0)*1 AS X_CONTROL,  IFNULL(A.Abono*1,0)  AS ENTREGADO,
A.Devolucion *1 AS DEVOLUCION, IFNULL(A.Basura*1,0) AS BASURA, A.Piocha  AS PIOCHAS,

                          (ifnull(OP.CantidadPiel1,0)  - (A.Abono*1))+(IFNULL(A.Basura*1,0)+(IFNULL(A.Devolucion*1,0))) AS DIFERENCIAX,
                       (OP.Pares * ((SELECT SUM(OPDD.Cantidad) FROM ordendeproducciond AS OPDD WHERE OPDD.OrdenDeProduccion = OP.ID AND OPDD.Articulo = OPD.Articulo) /OP.Pares))  - ((IFNULL(A.Abono,0) - IFNULL(A.Devolucion,0)))   AS DIFERENCIA,

                          (A.PrecioActual * SUM(OPD.Cantidad)) AS SISTEMA_PESOS,
                          (A.PrecioActual * (IFNULL(A.Abono,0)-(IFNULL(A.Basura,0)+IFNULL(A.Devolucion,0)))) AS REAL_PESOS,
                          (A.PrecioActual * (SUM(OPD.Cantidad) - A.Abono)+(IFNULL(A.Basura,0)+(IFNULL(A.Devolucion,0)))) AS DifPesos,
                          (A.Abono/OP.Pares) AS DCM2, (A.Abono/OP.Pares)/(SUM(OPD.Cantidad)/OP.Pares) AS PORCENTAJE

FROM ordendeproduccion AS OP INNER JOIN ordendeproducciond AS OPD ON OP.ID = OPD.OrdenDeProduccion
INNER JOIN asignapftsacxc AS A ON OP.ID = A.OrdenProduccion AND A.Articulo = OPD.Articulo
WHERE OP.Maquila = $P{MAQUILA} AND OP.Semana BETWEEN $P{SEMANAINICIO} AND $P{SEMANAFIN}  AND A.Empleado > 0 AND A.TipoMov = 1 AND
(CASE WHEN $P{ARTICULO} ="" THEN A.Articulo LIKE '%%' ELSE A.Articulo = $P{ARTICULO} END)
GROUP BY  OP.ControlT,  A.Articulo
ORDER BY ABS(A.Empleado) ASC,  ABS(OP.ControlT) ASC]]>
	</queryString>
