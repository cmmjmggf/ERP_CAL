	<queryString>
		<![CDATA[SELECT
C.docto AS DOCTO,
C.numcho AS CHOFER,
(SELECT CONCAT(
(CASE WHEN E.PrimerNombre = "0" THEN "" ELSE E.PrimerNombre END)
," ",
(CASE WHEN E.SegundoNombre = "0" THEN "" ELSE E.SegundoNombre END)
," ",
(CASE WHEN E.Paterno = "0" THEN "" ELSE E.Paterno END)
," ",
(CASE WHEN E.Materno = "0" THEN "" ELSE E.Materno END)
) AS NOMBRRE  FROM erp_cal.empleados AS E WHERE E.Numero = C.numcho) AS CHOFERT,
C.numtej AS TEJEDORA,
C.nomtej AS TEJEDORAT,
C.Control AS CONTROL,
C.estilo AS ESTILO,
C.color AS COLOR,
C.nomcolo AS COLORT,
C.pares AS PARES,
DATE_FORMAT(C.fechapre,"%d/%m/%Y") AS FECHA,
C.docto AS DOCUMENTO,
(SELECT S.Sem FROM semanasnomina AS S
WHERE C.fechapre BETWEEN STR_TO_DATE(S.FechaIni, "%d/%m/%Y") AND STR_TO_DATE(S.FechaFin, "%d/%m/%Y")
AND S.Sem <> '99' ) AS SEMANA_NOMINA
FROM controltej AS C INNER JOIN fracpagnomina AS F ON C.Control = F.Control AND F.fraccion in(401,402,403,404,405)
WHERE C.docto = $P{DOCUMENTO}]]>
	</queryString>
