	<queryString>
		<![CDATA[SELECT
P.Cliente AS CLIENTE,
C.RazonS AS CLIENTET, P.Clave AS PEDIDO,
P.Maquila AS MAQUILA, P.Semana AS SEMANA,
P.Estilo AS ESTILO, P.Color AS COLOR, P.ColorT AS COLORT,
P.Control AS CONTROL, P.Pares AS PARES, cast(P.Pares AS SIGNED) AS PARESC,
P.FechaPedido AS PEDIDO, P.FechaRecepcion AS CAPTURA,
P.FechaProg AS FECHA_PROD, P.FechaEntrega AS FECHA_ENTREGA,
DATEDIFF(DATE_FORMAT(str_to_date(P.FechaEntrega,"%d/%m/%Y"), "%Y-%m-%d"), NOW()) AS DIAS, P.stsavan AS AVANCE, P.Registro AS PEDIDOFE
FROM pedidox AS P INNER JOIN Clientes AS C ON P.Cliente = C.Clave WHERE DATEDIFF(DATE_FORMAT(str_to_date(P.FechaEntrega,"%d/%m/%Y"), "%Y-%m-%d"), NOW()) <= $P{DIAS} AND P.Ano =  $P{ANIO} AND P.Maquila BETWEEN $P{MAQUILA_INICIAL} AND $P{MAQUILA_FINAL}  ORDER BY P.Cliente ASC]]>
