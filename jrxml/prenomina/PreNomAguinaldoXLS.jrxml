	<queryString>
		<![CDATA[SELECT
PNL.ID, PNL.numsem, PNL.a単o, PNL.numemp,
(CASE
WHEN PNL.diasemp = 1 THEN 1
WHEN PNL.diasemp = 2 THEN 2
WHEN PNL.diasemp = 3 THEN 4
WHEN PNL.diasemp = 4 THEN 5
WHEN PNL.diasemp = 5 THEN 6
WHEN PNL.diasemp = 7 THEN 7
END) AS diasemp,
PNL.pares, PNL.salario, PNL.salariod, PNL.horext, PNL.otrper, PNL.otrper1, PNL.infon, PNL.imss, PNL.impu, PNL.precaha, PNL.cajhao, (IFNULL(PNL.vtazap,0)) AS vtazap, PNL.zapper, PNL.fune, PNL.cargo, PNL.fonac, PNL.otrde, IFNULL(PNL.fierabono,0) + IFNULL(PNL.otrde1,0) AS otrde1, PNL.registro, PNL.status, PNL.tpomov, PNL.salfis, PNL.depto,
IFNULL(D.Clave,999) AS NUMDEP, IFNULL(D.Descripcion,"NO EXISTE DEPTO") AS DEPARTAMENTO,
CONCAT(E.PrimerNombre,' ',
(CASE WHEN E.SegundoNombre <>'0' THEN E.SegundoNombre ELSE '' END),' ',
(CASE WHEN E.Paterno <>'0' THEN E.Paterno ELSE '' END),' ',
(CASE WHEN E.Materno <>'0' THEN E.Materno ELSE '' END)) AS EMPLEADO,
(CASE
WHEN E.Tarjeta = 1 THEN "C/T"
WHEN E.Tarjeta = 2 THEN "S/T"
END) AS TJ, E.FijoDestajoAmbos AS FDA,
E.Celula AS CELULA, E.CelulaPorcentaje AS CELULA_PORCENTAJE,
(CASE
WHEN E.FijoDestajoAmbos = 1 THEN E.Sueldo
WHEN E.FijoDestajoAmbos = 3 THEN E.Sueldo
ELSE 0 END) AS SAL_DI,
(PNL.salario + PNL.salariod + PNL.horext + PNL.otrper + PNL.otrper1) AS PERCEPCIONES,
    E.TBanbajio AS CUENTA_BB, DATE_FORMAT(E.FechaIngreso,"%d/%m/%Y") AS FECHA_INGRESO,
(CASE
WHEN E.FijoDestajoAmbos = 1 THEN "FJO"
WHEN E.FijoDestajoAmbos = 2 THEN "DJO"
WHEN E.FijoDestajoAmbos = 3 THEN "FI-DE"
END) AS FIJO_DESTAJO_AMBOS,
DATEDIFF(  NOW(),E.FechaIngreso ) AS DIAS_LABORADOS,
DATEDIFF(  NOW(),E.FechaIngreso ) / 365 AS DIAS_LABORADOS_365,
DATEDIFF(  NOW(),E.FechaIngreso ) / 30.41 AS DIAS_LABORADOS_3041,
(CASE WHEN DATEDIFF(  NOW(),E.FechaIngreso )> 30.41 THEN PNL.otrper1 ELSE 0 END) AS VACACIONES,

(CASE WHEN PNL.horext > 0.4 THEN (CASE WHEN DATEDIFF(NOW(),E.FechaIngreso)> 30.41 THEN PNL.otrper1 ELSE 0 END) * 0.25 ELSE 0 END) AS PRIMA,
(
(CASE WHEN DATEDIFF(  NOW(),E.FechaIngreso )> 30.41 THEN PNL.otrper1 ELSE 0 END) +
(CASE WHEN PNL.horext > 0.4 THEN (CASE WHEN DATEDIFF(NOW(),E.FechaIngreso)> 30.41 THEN PNL.otrper1 ELSE 0 END) * 0.25 ELSE 0 END)) AS TOTAL_GENERAL_X_EMPLEADO

FROM prenominal AS PNL INNER JOIN empleados AS E ON PNL.numemp = E.Numero LEFT JOIN departamentos AS D ON E.DepartamentoFisico = D.Clave
WHERE PNL.numsem = $P{semana} AND PNL.a単o = $P{anio} AND DATEDIFF(  NOW(),E.FechaIngreso ) >= 31
ORDER BY ABS(NUMDEP) ASC ,  ABS(E.Celula) ASC ,E.Busqueda ASC]]>
	</queryString>
