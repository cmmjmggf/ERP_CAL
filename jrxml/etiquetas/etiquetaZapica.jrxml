	<queryString>
		<![CDATA[select
E.Clave AS Estilo, L.Descripcion AS Linea,
concat(T.Descripcion,' ', E.Ano) as Temporada,
CASE
WHEN E.Corrida = '1' THEN '17 / 21'
WHEN E.Corrida = '2' THEN '22 / 27'
WHEN E.Corrida = '3' THEN '25 / 31'
END AS Corrida,
E.PuntoCentral,
'EE' AS Recio,
REPLACE(C.Descripcion,"/","\r\n") AS Materiales,
date_format(now(),'%d/%m/%Y') as fecha,

ifnull((SELECT A.Descripcion AS Suela from fichatecnica FT
join articulos A  on  FT.Articulo = A.Clave
where FT.Estilo = E.Clave  and FT.Color = C.Clave AND A.Grupo= 3
limit 1 ),'Z NO EXISTE SUELA EN FICHA TEC') AS Suela

from estilos E
inner join lineas L on L.Clave = E.Linea
inner join temporadas T on T.Clave = E.Temporada
inner join colores C on C.Estilo = E.Clave
where
E.linea = $P{Linea}
and E.Ano = $P{Ano}
and E.Temporada = $P{Temporada}
order by E.Clave, C.Clave ASC]]>
	</queryString>
