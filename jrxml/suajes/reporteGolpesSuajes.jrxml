	<parameter name="linea" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
'1 PIEL' AS Tipo,
(SELECT CONCAT(clave,' ',descripcion) FROM lineas WHERE clave = e.linea) AS linea,
e.estilo,
e.fechaaltasuajepiel AS fecha,
e.facturapiel,
e.proveedorpiel,
e.costopiel,
SUM(pe.pares) AS pares
FROM golpessuajesestilo e
JOIN pedidox pe ON pe.estilo = e.estilo AND pe.stsavan <> 14 AND pe.fechaproduccion >= e.fechaaltasuajepiel
WHERE e.linea = $P{linea}
GROUP BY e.estilo

UNION

SELECT
'2 FORRO/SINTÉTICOS' AS Tipo,
(SELECT CONCAT(clave,' ',descripcion) FROM lineas WHERE clave = e.linea) AS linea,
e.estilo,
e.fechaaltasuajeforro AS fecha,
e.facturaforro,
e.proveedorforro,
e.costoforro,
SUM(pe.pares) AS pares
FROM golpessuajesestilo e
JOIN pedidox pe ON pe.estilo = e.estilo AND pe.stsavan <> 14 AND pe.fechaproduccion >= e.fechaaltasuajeforro
WHERE e.linea = $P{linea}
GROUP BY e.estilo

ORDER BY Tipo, estilo ASC]]>
	</queryString>
