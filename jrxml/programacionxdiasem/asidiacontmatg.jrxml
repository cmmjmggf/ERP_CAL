	<queryString>
		<![CDATA[SELECT SUM(ordendeproducciond.Cantidad) AS CANTIDAD ,ordendeproducciond.Articulo AS ARTICULOX, ordendeproducciond.ArticuloT AS ARTICULOTX,

 CONCAT(empleados.`PrimerNombre`," ",(CASE WHEN empleados.`SegundoNombre` = "0" THEN "" ELSE empleados.`SegundoNombre` END)," ",empleados.`Paterno`," ",empleados.`Materno`) AS empleados_Nombre,
     empleados.`Numero` AS empleados_Numero,
     programacion.`frac` AS programacion_frac,
     programacion.`estilo` AS programacion_estilo,
     empleados.`AltaBaja` AS empleados_AltaBaja,
     UPPER(grupos.`Nombre`) AS grupos_Nombre,
     programacion.`control` AS programacion_control,
     ordendeproducciond.`Grupo` AS ordendeproducciond_Grupo,
     ordendeproducciond.`Articulo` AS ordendeproducciond_Articulo,
     ordendeproducciond.`ArticuloT` AS ordendeproducciond_ArticuloT,
     ordendeproducciond.`UnidadMedida` AS ordendeproducciond_UnidadMedida,
     ordendeproducciond.`UnidadMedidaT` AS ordendeproducciond_UnidadMedidaT,
       SUM(cast(ordendeproducciond.`Cantidad` as decimal(5,3))) AS ordendeproducciond_Cantidad

FROM ordendeproduccion AS O INNER JOIN  ordendeproducciond AS ordendeproducciond ON O.ID = ordendeproducciond.OrdenDeProduccion
INNER JOIN programacion AS programacion ON programacion.control = O.ControlT INNER JOIN empleados AS empleados ON programacion.numemp = empleados.Numero  INNER JOIN `grupos` grupos ON ordendeproducciond.`Grupo` = grupos.`Clave`
WHERE  programacion.semana = $P{SEMANA}  AND programacion.año = $P{ANO} AND programacion.semana = $P{SEMANA}
 AND programacion.diaprg = $P{DIA}
GROUP BY  ordendeproducciond.`Grupo`, programacion.numemp,ordendeproducciond.Articulo
ORDER BY ABS(ordendeproducciond.`Grupo`) ASC ,ABS(ordendeproducciond.Articulo) ASC]]>
	</queryString>
