	<queryString>
		<![CDATA[SELECT
ag.clave, ag.Descripcion as nomagente, cp.tipo, cp.cliente, ct.RazonS as nomcliente, cc.remicion,
cc.fecha as fechadoc, datediff(cp.fecha,cc.fecha) as dias, cp.fechacap, cp.fecha,
cp.importe, cp.doctopa
from cartctepagos cp
join cartcliente cc on cp.cliente = cc.cliente and cp.tipo = cc.tipo and cp.remicion = cc.remicion
join clientes ct on cp.cliente = ct.clave
join agentes ag on ag.clave = ct.agente
where cp.mov = 2
and cp.fechacap between $P{fechaIni} and $P{fechaFin}
order by abs(ct.Agente),cp.tipo, cp.cliente, cp.fecha asc]]>
	</queryString>
