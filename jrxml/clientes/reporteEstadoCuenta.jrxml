	<queryString>
		<![CDATA[select cc.cliente,
ifnull(ct.RazonS,'') as nomcliente, ct.DiasPlazo, ct.TelOficina,
cc.tipo, cc.remicion, cc.fecha,
cc.importe,
cc.pagos,
cc.saldo,
case when cc.nc = 999 then 'ANTICIPO' end as anticipos,
date_add(cc.fecha, INTERVAL ct.DiasPlazo DAY) as fechavencimiento,
datediff(now(), cc.fecha) as dias, cc.tmnda, cc.tcamb
from cartcliente cc
left join clientes ct on ct.clave = cc.cliente
where cc.cliente between $P{dCliente} and $P{aCliente}
and cc.tipo like concat('%',$P{tp},'%')
and cc.status < 4
and cc.saldo > 4
order by cc.tipo, cc.cliente, cc.remicion asc]]>
	</queryString>
