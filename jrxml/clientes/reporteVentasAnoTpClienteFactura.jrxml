	<queryString>
		<![CDATA[select
(select concat(e.linea,' ',l.descripcion) from estilos e join lineas l on l.clave = e.linea where e.clave like TRIM(REPLACE(fa.estilo, CHAR(13,10) , ''))) as linea,
(select descripcion from colores where estilo like TRIM(REPLACE(fa.estilo, CHAR(13,10) , '')) and clave = fa.combin) as nomcolor,
fa.cliente,
(select razons from clientes where clave = fa.cliente) as nomcliente,
TRIM(REPLACE(fa.estilo, CHAR(13,10) , '')) AS estilo,
fa.combin,
sum(case when month(fa.fecha) = 1 then fa.pareped end) as ene,
sum(case when month(fa.fecha) = 2 then fa.pareped end) as feb,
sum(case when month(fa.fecha) = 3 then fa.pareped end) as mar,
sum(case when month(fa.fecha) = 4 then fa.pareped end) as abr,
sum(case when month(fa.fecha) = 5 then fa.pareped end) as may,
sum(case when month(fa.fecha) = 6 then fa.pareped end) as jun,
sum(case when month(fa.fecha) = 7 then fa.pareped end) as jul,
sum(case when month(fa.fecha) = 8 then fa.pareped end) as ago,
sum(case when month(fa.fecha) = 9 then fa.pareped end) as sep,
sum(case when month(fa.fecha) = 10 then fa.pareped end) as oct,
sum(case when month(fa.fecha) = 11 then fa.pareped end) as nov,
sum(case when month(fa.fecha) = 12 then fa.pareped end) as dic
from facturacion fa
where
fa.cliente = $P{cliente}
and year(fa.fecha) = $P{ano}
and fa.tp = $P{tp}
and fa.staped < 3
group by fa.estilo, fa.combin
order by fa.cliente, linea, estilo, fa.combin asc]]>
	</queryString>
