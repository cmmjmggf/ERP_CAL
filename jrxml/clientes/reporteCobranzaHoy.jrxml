	<queryString>
		<![CDATA[select cc.cliente, cte.RazonS as nomcliente, cte.DiasPlazo, cc.remicion, cte.agente,
cc.fecha as fechaFactura,

date_add(cc.fecha, INTERVAL cte.DiasPlazo DAY) as fechaLiquidacion,
cc.importe, cc.pagos, cc.saldo,
datediff(now(),cc.fecha) as dias
from cartcliente cc
join clientes cte on cte.clave = cc.cliente
where cc.status < 3
and cte.agente between $P{dAgente} and $P{aAgente}
order by cc.cliente asc, cc.fecha asc, cc.remicion asc]]>
	</queryString>
