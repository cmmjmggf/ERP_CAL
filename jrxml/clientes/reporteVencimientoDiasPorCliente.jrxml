	<parameter name="cliente" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select
pe.Cliente,
ct.RazonS as nomcliente,
pe.Clave as pedido,
pe.Maquila,
pe.Semana,
pe.Estilo,
pe.Color,
pe.ColorT,
pe.Control,
pe.Pares,
pe.FechaPedido,
pe.Registro,
pe.FechaProduccion,
pe.FechaEntrega,
datediff(str_to_date(pe.FechaEntrega,'%d/%m/%Y'),now()) as dias,
pe.stsavan,
case
when pe.stsavan=1 then 'Programado'
when pe.stsavan=2 then 'Corte'
when pe.stsavan=3 then 'Rayado'
when pe.stsavan=33 then 'Rebajado'
when pe.stsavan=4 then 'Foleado'
when pe.stsavan=40 then 'Entretelado'
when pe.stsavan=42 then 'Procesos maquila'
when pe.stsavan=44 then 'Alm-corte'
when pe.stsavan=5 then 'Pespunte'
when pe.stsavan=55 then 'Ensuelado'
when pe.stsavan=6 then 'Almacen pesp'
when pe.stsavan=7 then 'Tejido'
when pe.stsavan=8 then 'Almacen tejido'
when pe.stsavan=9 then 'Montado'
when pe.stsavan=10 then 'Adorno'
when pe.stsavan=11 then 'Almacen adorno'
when pe.stsavan=12 then 'Prd-Terminado'
when pe.stsavan=14 then 'Cancelado' end as desava,
case when pe.stsavan < 11 then 'Alta prioridad' else 'Media prioridad' end as prio
from pedidox pe
join clientes ct on ct.clave = pe.cliente
where pe.stsavan in (42, 1, 10, 11, 12, 2, 3, 33, 4, 44, 5, 55, 6, 7, 8, 9)
and pe.cliente = $P{cliente}
order by cast(pe.Cliente as signed), pe.control asc]]>
	</queryString>
