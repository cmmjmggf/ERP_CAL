	<queryString>
		<![CDATA[select ct.maq, m.Nombre as nomaq,
ct.docto,ct.sem, ct.control, ct.estilo, ct.color, pe.ColorT as nomcolor,
pe.cliente, cte.RazonS as nomcliente,
ct.fecha, pe.FechaEntrega, datediff(str_to_date(pe.FechaEntrega,'%d/%m/%Y'),ct.fecha) as dias,
cast(pe.Pares as signed) as pares,
lpm.PrecioVta as precio,
cast(ifnull(fe.CostoVTA,10)as decimal(7,2)) as precioTej,
TRUNCATE((lpm.PrecioVta + cast(ifnull(fe.CostoVTA,10)as decimal(7,0))) * cast(pe.Pares as signed),0) as total,
(lpm.PrecioVta + cast(ifnull(fe.CostoVTA,10)as decimal(7,0))) * cast(pe.Pares as signed) as total2
from controlterm ct
join maquilas m on m.clave = ct.maq
join fraccionesxestilo fe on fe.estilo like ct.estilo and fe.fraccion = 401
join pedidox pe on pe.control = ct.control
join clientes cte on cte.clave = pe.cliente
join listapreciosmaquilas lpm on lpm.maq = ct.maq and lpm.estilo = ct.estilo and lpm.Color = ct.color
where ct.fecha between $P{fechaIni} and $P{fechaFin}
order by ct.maq, ct.docto, abs(ct.sem), ct.control asc;]]>
	</queryString>
