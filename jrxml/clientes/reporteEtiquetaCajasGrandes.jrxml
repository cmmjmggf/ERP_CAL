	<parameter name="transporte" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
-- cc.remicion,
(select case when cajas = 0  then 1 else cajas end from facturacion where cliente= cc.cliente and tp = cc.tipo and factura = cc.remicion limit 1) as cajas,
-- case when cc.tipo = 1 then cc.importe/1.16 when cc.tipo = 2 then cc.importe end as Importe,
cte.RazonS as nomcliente,
concat(cte.Direccion,' Ext. ',cte.NoExt,' Int. ',cte.NoInt) as direccion ,
cte.Colonia,
cte.Ciudad,
edo.Descripcion as edo,
cte.CodigoPostal,
cte.RFC,
cte.TelOficina,
tp.Descripcion as numtra
FROM cartcliente cc
join clientes cte on cte.clave = cc.cliente
join estados edo on edo.clave = cte.estado
join transportes tp on tp.clave = cte.transporte
where cc.cliente= $P{cliente} and cc.tipo = $P{tp} and cc.remicion = $P{factura};]]>
	</queryString>
