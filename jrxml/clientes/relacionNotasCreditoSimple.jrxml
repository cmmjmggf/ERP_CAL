	<queryString>
		<![CDATA[select nc.nc, nc.cliente, ct.RazonS as nomcliente, ct.rfc, nc.numfac, nc.fecha, nc.cant, nc.descripcion, nc.subtot, nc.orden,
case
when nc.orden = 5 then 'Descuento'
when nc.orden = 6 then 'Devoluciones'
when nc.orden = 7 then 'Diferencia precio'
when nc.orden = 9 then 'Otros y tiendas'
when nc.orden = 10 then 'Refacturación' end as tipo
from notcred nc
join clientes ct on ct.Clave = nc.cliente
where nc.tp = $P{tp}
and nc.fecha between $P{fechaIni} and $P{fechaFin}
and nc.status < 3
order by nc.orden, nc.nc asc;]]>
	</queryString>
