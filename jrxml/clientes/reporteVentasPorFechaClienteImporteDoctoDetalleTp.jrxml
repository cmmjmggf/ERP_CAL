	<queryString>
		<![CDATA[select
cast(ag.clave as signed) as numagente,
ag.nombre as nomagente,
case when
(select nc from cartcliente where remicion = fa.factura and cliente = fa.cliente and tipo = fa.tp limit 1) = 999 then 'ANTICIPO' else ''
end as anticipo,
fa.cliente,
ct.RazonS as nomcliente,
fa.tp,
fa.factura,
fa.estilo,
fa.fecha,
fa.precto,
sum(case when fa.staped = 3 then 0 else fa.pareped end) as pares,
(CASE WHEN fa.zero = 1 THEN 0 ELSE
sum(case when fa.staped = 3 then 0
else
	case when fa.tmnda <> 1 then fa.subtot*fa.tcamb else fa.subtot end
end) END) as subtotal,
case when fa.staped = 3 then 'CANCELADA' end as cancelada,
fa.tmnda,
fa.tcamb,
IFNULL(fa.zero ,0) AS zero_apoyo
from facturacion fa
join clientes ct on ct.clave = fa.cliente
join agentes ag on ag.clave= ct.agente
where fa.factura > 0
and fa.origen <> 9999
and
case
when $P{tp} = '1' then fa.tp = 1
when $P{tp} = '2' then fa.tp = 2
ELSE fa.tp LIKE '%%'
end
and fa.fecha between $P{fechaIni} and $P{fechaFin}
group by fa.factura
order by numagente,fa.cliente, fa.factura asc]]>
	</queryString>
