	<queryString>
		<![CDATA[select
case when
(select nc from cartcliente where remicion = fa.factura and cliente = fa.cliente and tipo = fa.tp) = 999 then 'ANTICIPO' else ''
end as anticipo,
ag.clave as numagente,
ag.Nombre as nomagente,
fa.cliente,
ct.RazonS as nomcliente,
fa.tp,
fa.factura,
fa.contped,
fa.estilo,
fa.fecha,
fa.precto,
case when fa.staped = 3 then 0 else fa.pareped end as pares,
case when fa.staped = 3 then 0
else
	case when fa.tmnda <> 1 then fa.subtot*fa.tcamb else fa.subtot end
end as subtotal,
case when fa.staped = 3 then 'CANCELADA' end as cancelada,
fa.tmnda,
fa.tcamb
from facturacion fa
join clientes ct on ct.clave = fa.cliente
join agentes ag on ag.clave = ct.Agente
where fa.factura > 0
and fa.origen <> 9999
and
case
when $P{tp} = '1' then fa.tp = 1
when $P{tp} = '2' then fa.tp = 2
ELSE fa.tp LIKE '%%'
end
and fa.fecha between $P{fechaIni} and $P{fechaFin}
order by abs(ag.clave), fa.factura, fa.contped asc]]>
	</queryString>
