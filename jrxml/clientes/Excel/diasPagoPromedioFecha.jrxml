	<queryString>
		<![CDATA[select cp.tipo,cp.cliente, ct.RazonS as nomcliente, cp.remicion, cc.fecha, cp.fechadep, datediff(cp.fechadep,cc.fecha) as dias
from cartctepagos cp
join cartcliente cc on cc.cliente = cp.cliente and cc.tipo = cp.tipo and cc.remicion = cp.remicion
join clientes ct on ct.clave = cp.cliente
where cp.fechadep between $P{fechaIni} and $P{fechaFin}
and cp.mov < 5
order by cp.tipo asc, cp.cliente asc,cp.fecha asc]]>
