	<queryString>
		<![CDATA[SELECT
ag.Clave as numagente,
ag.Nombre nomagente,
ct.clave as numcte,
ct.RazonS as nomcte,
pe.clave as pedido,
pe.Control,
pe.Estilo,
pe.ColorT,
cast(pe.Pares as signed) as pares,
cast(pe.ParesFacturados as signed) as paresFacturados,
pe.Pares - ifnull(pe.ParesFacturados,0) as saldo,
pe.Registro as fecped,
pe.FechaEntrega as fecent,
av.fec12 as fecterm,
ct.Observaciones,
pe.Observacion
from pedidox pe
join clientes ct on ct.clave = pe.cliente
join agentes ag on ag.clave = ct.agente
left join avaprd av on av.contped = pe.Control
where pe.stsavan = 12
and pe.Maquila <> '98'
and (pe.Pares - ifnull(pe.ParesFacturados,0)) > 0
order by abs(numagente), abs(numcte),pe.Estilo, pe.ColorT  asc;]]>
	</queryString>
