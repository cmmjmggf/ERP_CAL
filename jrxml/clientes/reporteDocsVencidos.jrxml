	<queryString>
		<![CDATA[select cc.cliente, cte.RazonS as nomcliente, cte.DiasPlazo, cte.Descuento, cte.TelOficina,
cc.tipo, cc.remicion,
cc.fecha as fechaFactura, date_add(cc.fecha, INTERVAL cte.DiasPlazo DAY) as fechaLiquidacion,
cc.importe, cc.pagos, cc.saldo,
datediff(now(),cc.fecha) as dias
from cartcliente cc
join clientes cte on cte.clave = cc.cliente
where cc.status < 3 and datediff(now(),cc.fecha) > cast(cte.DiasPlazo as signed)
order by cc.cliente asc, cc.fecha asc, cc.remicion asc]]>
	</queryString>
