	<queryString>
		<![CDATA[select
ct.cliente, c.RazonS as nomcliente,
ct.remicion,
ct.fecha,
ct.importe,
ct.pagos,
ct.saldo,
c.Agente as numagente,
agt.Nombre as nomagente,
ct.tipo
from cartcliente ct
join clientes c on c.Clave = ct.cliente
join agentes agt on agt.clave = c.Agente
where ct.status < 3
and c.Agente = $P{agente}
and ct.fecha between $P{fechaIni} and $P{fechaFin}
and ct.tipo = $P{tp}
order by ct.cliente,ct.fecha,ct.remicion]]>
	</queryString>
