	<parameter name="maq" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
CT.control, PE.Estilo, PE.Color, PE.ColorT,
PE.FechaEntrega, CAST(PE.Pares AS DECIMAL(8,0)) AS Pares,
CAST(CT.prevta AS DECIMAL(8,2)) AS prevta,
PE.Pares * CT.prevta AS Subtotal,
PE.Serie as SerieCorrida,
S.T1 as S1,S.T2 as S2,S.T3 as S3,S.T4 as S4,S.T5 as S5,S.T6 as S6,S.T7 as S7,S.T8 as S8,S.T9 as S9,S.T10 as S10,S.T11 as S11,S.T12 as S12,S.T13 as S13,
PE.C1,PE.C2,PE.C3,PE.C4,PE.C5,PE.C6,PE.C7,PE.C8,PE.C9,PE.C10,PE.C11,PE.C12,PE.C13,PE.C14,
PE.C15,PE.C16,PE.C17,PE.C18,PE.C19,PE.C20,PE.C21,PE.C22 ,
PE.Semana, PE.stsavan as avance
FROM controlterm CT
JOIN pedidox PE ON PE.control =  CT.Control and PE.Estatus <> 'C'
JOIN series S on S.Clave = PE.Serie
WHERE Maquila = $P{maq} AND CT.Docto = $P{docto}
ORDER BY PE.Semana ASC, PE.Serie ASC, CT.Control ASC]]>
	</queryString>
