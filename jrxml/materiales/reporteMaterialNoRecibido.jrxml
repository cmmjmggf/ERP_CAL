	<queryString>
		<![CDATA[SELECT
oc.Folio as oc,
oc.proveedor as numprov,
ifnull((select nombref from proveedores where clave = oc.proveedor),'') as nomprov ,
oc.cantidad,
oc.cantidadrecibida,
u.Descripcion as unidad,
oc.articulo as numart,
a.descripcion as nomart,
oc.Precio as precio,
oc.SubTotal as subt,
oc.ConsignarA as consig,
oc.ano,
oc.sem,
oc.maq,
oc.FechaOrden as fechacap,
oc.FechaEntrega as fecent
from ordencompra oc
join articulos a on oc.articulo = a.clave
join unidades u on u.clave = a.unidadmedida
where oc.estatus in ('ACTIVA', 'PENDIENTE')
order by oc.folio asc]]>
	</queryString>
