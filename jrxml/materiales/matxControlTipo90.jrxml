	<queryString>
		<![CDATA[SELECT `OP`.`ControlT`, `OPD`.`Articulo`, `OPD`.`ArticuloT`, `OPD`.`UnidadMedidaT`, sum(OPD.Cantidad) AS Cantidad
FROM `pedidox` AS `PE`
JOIN `ordendeproduccion` AS `OP` ON `PE`.`Control` = `OP`.`ControlT` AND `PE`.`stsavan` <> 14
JOIN `ordendeproducciond` AS `OPD` ON `OP`.`ID` = `OPD`.`OrdenDeProduccion`
WHERE `PE`.`ano` = $P{Ano}
AND `PE`.`semana` = $P{Sem}
AND `PE`.`maquila` = $P{Maq}
AND `OPD`.`DepartamentoArt` IN('90')
AND OPD.Grupo IN ('10', '22', '32', '38', '39','8')
GROUP BY `OP`.`ControlT`, `OPD`.`Articulo`
ORDER BY  `OP`.`ControlT` ASC ,`OPD`.`ArticuloT` ASC]]>
	</queryString>
