	<queryString>
		<![CDATA[SELECT
CAST(A.Grupo AS SIGNED ) AS ClaveGrupo,
(SELECT G.Nombre FROM grupos G WHERE clave = A.Grupo) AS NomGrupo,
A.Clave AS ClaveArt,
A.Descripcion AS Articulo,
U.Descripcion AS Unidad,
cast(ELT($P{mes},A.`PEne`, A.`PFeb`,A.`PMar`,A.`PAbr`,A.`PMay`,A.`PJun`,A.`PJul`,A.`PAgo`,A.`PSep`,A.`POct`,A.`PNov`,A.`PDic`) as decimal(10,2)) AS Costo,
A.Existencia as Existencia,
cast(ELT($P{mes},A.`PEne`, A.`PFeb`,A.`PMar`,A.`PAbr`,A.`PMay`,A.`PJun`,A.`PJul`,A.`PAgo`,A.`PSep`,A.`POct`,A.`PNov`,A.`PDic`) as decimal(10,2)) * A.Existencia AS Total
FROM articulos10 A
JOIN unidades U ON U.Clave = A.UnidadMedida
WHERE IFNULL(A.Existencia,0) > 0
ORDER BY ClaveGrupo,A.Descripcion ASC]]>
	</queryString>
