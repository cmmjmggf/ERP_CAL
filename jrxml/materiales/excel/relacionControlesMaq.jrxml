	<queryString>
		<![CDATA[SELECT
C.Control,
C.Estilo,
C.Color,
C.ColorT AS Descripcion,
CAST(C.DeptoProduccion AS SIGNED)  AS ClaveDepto,
C.EstatusProduccion AS NombreDepto,
C.Pares,
C.FechaEntrega,
C.FechaProduccion AS FechaProg,
LPM.PrecioVta,
IFNULL(LPM.PrecioVta,0) * C.Pares AS Subtotal,
CAST(FXE.CostoMO AS DECIMAL(10,2)) AS CostoTejido,
IFNULL(FXE.CostoMO,0) * C.Pares AS SubtotalTej,
(IFNULL(FXE.CostoMO,0) * C.Pares) + (IFNULL(LPM.PrecioVta,0) * C.Pares) AS Neto,
CT.docto AS DoctoTerm,
CT.fecha AS FechaTerm
FROM pedidox C
LEFT JOIN listapreciosmaquilas LPM ON C.Maquila = LPM.Maq AND C.Estilo = LPM.Estilo AND C.Color =  LPM.Color
LEFT JOIN fraccionesxestilo FXE ON FXE.Estilo = C.Estilo AND FXE.Fraccion = '401'
LEFT JOIN controlterm CT ON CT.Control =  C.Control
WHERE C.Maquila = $P{maq} AND C.Semana = $P{sem} AND C.Ano = $P{ano} and C.stsavan <> 14
order by ClaveDepto ASC, C.Control ASC
]]>
