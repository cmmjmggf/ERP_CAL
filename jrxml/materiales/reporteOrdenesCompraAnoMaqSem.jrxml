	<queryString>
		<![CDATA[-- TODO
SELECT
cast(a.grupo as signed) as numgrupo, g.Nombre as nomgrupo,
cast(oc.Proveedor as signed) as numprov, case when oc.tp = 1 then p.NombreF else p.nombreI end as nomprov,
oc.articulo, a.Descripcion as nomarticulo,
oc.Folio, oc.FechaOrden, oc.FechaEntrega, oc.Factura,
oc.Cantidad, oc.CantidadRecibida,  oc.Cantidad - oc.CantidadRecibida as saldo,
oc.Precio, oc.SubTotal,
case
when oc.Estatus = 'ACTIVA' THEN 'Sin Recibir'
when oc.Estatus = 'PENDIENTE' THEN 'Parcial'
when oc.Estatus = 'RECIBIDA' THEN 'Recibida'
end as estatus,
oc.Ano,
oc.Sem,
oc.Maq
FROM ordencompra oc
join articulos a on a.Clave = oc.Articulo
join grupos g on g.clave = a.grupo
join proveedores p on p.clave = oc.proveedor
where oc.ano = $P{ano}
and oc.tipo = $P{tipo}

and (case when $P{csaldo}= '1' then  oc.Estatus in ('ACTIVA','PENDIENTE')
else oc.Estatus in ('ACTIVA','PENDIENTE','RECIBIDA') end)

and (case when $P{proveedor} <> '' then  oc.Proveedor = $P{proveedor}  else  oc.Proveedor like '%%' end)
and cast(oc.maq as signed) between $P{maq} and $P{amaq}
and cast(oc.sem as signed) between $P{sem} and $P{asem}
order by numprov, numgrupo, a.Descripcion asc;]]>
	</queryString>
