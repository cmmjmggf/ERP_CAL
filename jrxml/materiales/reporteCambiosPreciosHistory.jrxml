	<queryString>
		<![CDATA[SELECT  a.clave as numart, a.Descripcion as nomart,
ifnull(a.ProveedorUno,'') as ProveedorUno, ifnull(a.ProveedorDos,'') as ProveedorDos, ifnull(a.ProveedorTres,'') as ProveedorTres,
ifnull((select p.NombreF from proveedores p where clave = a.ProveedorUno),'') as nomprov1,
ifnull((select p.NombreF from proveedores p where clave = a.ProveedorDos),'') as nomprov2,
ifnull((select p.NombreF from proveedores p where clave = a.ProveedorTres),'') as nomprov3,
h.fecha, h.usuario,
preciounoant,
preciounonuevo,
preciodosant,
preciodosnuevo,
preciotresant,
preciotresnuevo
FROM articuloshistory h join
articulos a on a.Clave = h.Articulo
where a.clave = $P{articulo}
order by h.fecha desc]]>
	</queryString>
