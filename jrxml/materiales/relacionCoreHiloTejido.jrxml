	<queryString>
		<![CDATA[select pe.control as ControlT, pe.Semana, pe.Estilo, pe.Color, pe.ColorT, pe.Pares,
a.clave as Articulo, a.Descripcion as ArticuloT , ft.Consumo,
ft.Consumo/2 AS CorteMetro, pe.Pares * 2 AS Hembras,
pe.Pares*ft.Consumo AS ConsumoTotal
from pedidox pe
join fichatecnica ft on ft.Estilo = pe.Estilo and ft.Color = pe.Color and ft.Pieza = '1139'
join articulos a on a.clave = ft.Articulo
where pe.ano = $P{ano} and pe.semana = $P{sem} and pe.maquila = $P{maq} and pe.stsavan <> 14
ORDER BY ArticuloT ASC, pe.Estilo ASC, pe.Color ASC]]>
	</queryString>
