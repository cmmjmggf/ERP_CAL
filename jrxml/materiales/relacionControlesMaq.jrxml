	<queryString>
		<![CDATA[SELECT
C.Control,
C.Estilo,
C.Color,
CO.Descripcion,
CAST(D.Clave AS SIGNED)  AS ClaveDepto,
D.Descripcion AS NombreDepto,
C.Pares,
P.FechaEntrega,
P.FechaProduccion as FechaProg,
LPM.PrecioVta,
ifnull(LPM.PrecioVta,0) * C.Pares AS Subtotal,
CAST(FXE.CostoMO AS DECIMAL(10,2)) AS CostoTejido,
ifnull(FXE.CostoMO,0) * C.Pares AS SubtotalTej,
(ifnull(FXE.CostoMO,0) * C.Pares) + (ifnull(LPM.PrecioVta,0) * C.Pares) AS Neto,
CT.docto AS DoctoTerm,
CT.fecha AS FechaTerm
FROM controles C
INNER JOIN colores CO ON C.Estilo= CO.Estilo AND C.Color = CO.Clave
INNER JOIN pedidox P ON P.Control = C.Control
LEFT JOIN departamentos D ON D.Descripcion = C.EstatusProduccion
LEFT JOIN listapreciosmaquilas LPM ON P.Maquila = LPM.Maq AND C.Estilo = LPM.Estilo AND C.Color =  LPM.Color
LEFT JOIN fraccionesxestilo FXE ON FXE.Estilo = C.Estilo AND FXE.Fraccion = '401'
LEFT JOIN controlterm CT ON CT.Control =  C.Control
WHERE P.Maquila = $P{maq} AND P.Semana = $P{sem} AND P.Ano = $P{ano}
order by ClaveDepto ASC, C.Control ASC]]>
	</queryString>
