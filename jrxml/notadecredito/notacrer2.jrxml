	<queryString>
		<![CDATA[SELECT NC.nc, NC.cliente, NC.numfac, NC.tp, NC.orden, NC.fecha, NC.hora, NC.cant, NC.descripcion, NC.precio, NC.subtot, NC.concepto, NC.status, NC.monletra, NC.defecto, NC.detalle, NC.registro, NC.tmnda, NC.tcamb, NC.uuid,
(CASE WHEN NC.tp = 1 THEN C.RazonS ELSE C.NombreP END) AS CLIENTE_NOMBRE,
CONCAT(C.Direccion," ",C.Colonia) AS CLIENTE_DIRECCION, C.Ciudad AS CLIENTE_CIUDAD,
C.Estado AS CLIENTE_CLAVE_ESTADO, E.Descripcion AS NOMBRE_ESTADO, C.RFC AS CLIENTE_RFC, C.CodigoPostal AS CLIENTE_CODIGO_POSTAL, D.Descripcion AS DEFECTO_NOMBRE, DD.Descripcion AS DETALLEDEFECTO_NOMBRE

FROM notcred as NC INNER JOIN clientes AS C ON NC.cliente = C.Clave
INNER JOIN estados AS E ON C.Estado = E.Clave INNER JOIN secciones AS S ON C.Agente = S.numsec
INNER JOIN defectos AS D ON NC.defecto = D.Clave INNER JOIN defectosdetalle AS DD ON NC.detalle = DD.Clave
WHERE NC.tp = 2 AND S.tipo = 1]]>
	</queryString>
