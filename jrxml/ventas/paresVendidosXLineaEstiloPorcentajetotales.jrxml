	<queryString>
		<![CDATA[select
case
when pe.serie = '1' then 'CABALLERO'
when pe.serie = '2' then 'DAMA'
when pe.serie = '3' then 'NIÃ‘O/A'
END AS Genero,
l.descripcion as linea,
pe.estilo,
sum(pe.pares) as pares, (SELECT sum(px.pares) from pedidox px
join estilos e on e.clave = px.estilo
join lineas l on l.clave = e.linea
where str_to_date(px.FechaEntrega,'%d/%m/%Y') between $P{fechaIni} and $P{fechaFin}
and px.stsavan <> 14 and px.maquila <> 98  limit 1) as pares_totales
from pedidox pe
join estilos e on e.clave = pe.estilo
join lineas l on l.clave = e.linea
where str_to_date(pe.FechaEntrega,'%d/%m/%Y') between $P{fechaIni} and $P{fechaFin}
and pe.stsavan <> 14 and pe.maquila <> 98
group by pe.Estilo
order by abs(pe.serie), linea asc, (pares) desc]]>
	</queryString>
